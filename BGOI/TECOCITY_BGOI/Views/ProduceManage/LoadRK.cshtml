                <td>库管员:</td>
                <td>@ViewData["Storekeeper"] </td>
            </tr>
            <tr>
                <td>入库人:</td>
                <td>@ViewData["StockInUser"] </td>
            </tr>
        </table>
    </div>
    <div id="bor1" style="width: 100%; margin-top: 5px; margin-left: 5px;">
        <table cellpadding="0" cellspacing="0" border="0" id="list1" class="scroll" style="width: 100%;"></table>
