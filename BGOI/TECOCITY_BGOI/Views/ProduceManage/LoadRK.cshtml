     <div id="RK" style="display: none">
        <table class="tabInfo" cellpadding="0" style="width: 98%; text-align: center">
            <tr>
                <td>完成日期:</td>
                <td>@ViewData["FinishTime"]  </td>
                <td>入库日期:</td>
                <td>@ViewData["StockInTime"] </td>
                <td>入库库房:</td>
                <td>@ViewData["HouseID"] </td>
