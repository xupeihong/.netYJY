@using TECOCITY_BGOI
@{
    ViewBag.Title = "PrintShipments";
}
@model Shipments
<!DOCTYPE html>

<html>
<head>
    @*  <meta name="viewport" content="width=device-width" />*@
    <title></title>
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery_dialog.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/ui.jqgrid.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.jqGrid.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/My97DatePicker/WdatePicker.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/cmm_PrintPageSet.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Sales/PrintShipments.js")" type="text/javascript"></script>
    <style media="print">
        .Noprint {
            display: none;
        }

        #list {
            width: 100%;
        }
    </style>
</head>
<body style="margin: 0 0 0 0; background-color: white; overflow: auto;">
    <div id="">发货单编号：@Model.ShipGoodsID</div>
    <div id="PrintArea" style="page-break-after: always;" >
        <div id="" style="text-align:center;font-size:18px; margin-top:20px;">
            北 京 市 燕 山 工 业 燃 气 设 备 有 限 公 司
        </div>
        <div id="" style="text-align:center;font:900;font-size:18px;">
            发 货 通 知 单
        </div>
        <div id="" style="margin-left:10px;font-size:16px;">
            @*<table><tr><td> 合同编号:@ViewData["ContractID"]</td><td>YSGLJL-XS-F03</td></tr>
            </table>*@
            合同编号:@ViewData["ContractID"] &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;YSGLJL-XS-F03
        </div>
        <hr style="width:98%" />
        <div id="bor" style="width:100%; margin-top: 5px; margin-left: 5px;">
            <table id="btnShipmentDetail" class="tabInfoP" style=" width :98% ;">
                <tr align="center" valign="middle">
                    <th style="width: 15%;" class="th">
                        序号
                    </th>
                    <th style="width: 25%;" class="th">
                        物品名称
                    </th>
                    <th style="width: 25%;" class="th">
                        规格型号
                    </th>
                    <th style="width:25%;" class="th">
                        数量
                    </th>
                </tr>
                <tbody id="DetailInfo" class="tabInfoP"></tbody>
                <tbody id="Shipment" class="tabInfoP"></tbody>
            </table>
        </div>
        <div>
            <input type="button" style="margin-right:10px;" class="btn" value="打印" id="btnPrint" />
        </div>
    </div>
</body>
</html>

