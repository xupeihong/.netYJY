@{
    ViewBag.Title = "ExchangeGoodsFinish";
}

@using TECOCITY_BGOI
@model ExchangeGoods
<!DOCTYPE html>
<html>
<head>
    <title></title>
    <link href="@Url.Content("~/CSS/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery_dialog.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/ui.jqgrid.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Validate/Validate-Injection.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.jqGrid.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/My97DatePicker/WdatePicker.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Sales/ExchangeGoodsFinish.js")" type="text/javascript"></script>
    <style>
        .RowClass {
            background-color: #efd2d2;
        }
    </style>
</head>
<body>
    <form id="form1" method="post">
        <div id="tabTitile"><span style="margin-left: 10px; float: left;">退换货编号：@Model.EID</span></div>@Html.HiddenFor(model => Model.EID)
        <div>订单编号：@Model.OrderID   @Html.HiddenFor(model => Model.OrderID)</div>
        <div style="overflow-y:auto;">
            <div>
                <table id="myTable" cellpadding="0" cellspacing="0" class="tabInfo" style=" width :98% ;">
                    <tr>
                        <td>退货完成日期：</td>
                        <td>@Html.TextBoxFor(model => Model.ExFinishDate, new { @class = "txtCss", @readonly = "readonly", Value = String.Format("{0:yyyy-MM-dd HH:mm:ss}", DateTime.Now), onclick = "WdatePicker({skin:'whyGreen',dateFmt:'yyyy-MM-dd HH:mm:ss'})" })<span style="color:red;"> * </span>@Html.ValidationMessageFor(model => model.ExFinishDate)</td>
                        
                    </tr>
                    <tr>
                        <td>退货完成说明：</td>
                        <td>@Html.TextBoxFor(model => Model.ExFinishDescription)<span style="color:red;"> * </span>@Html.ValidationMessageFor(model => model.ExFinishDescription)</td>
                    </tr>
                    <tr>
                        <td>记录人：</td>
                        <td colspan=" 3">@Html.TextBoxFor(model => Model.ExFinishDealPeo)<span style="color:red;"> * </span>@Html.ValidationMessageFor(model => model.ExFinishDealPeo)</td>

                    </tr>
                </table>
            </div>
        </div>
        <input type="button" class="btn" id="btnSaveExChangeFinish" value="保存" />
        <input type="button" class="btn" id="btnExit" value="取消" />
    </form>
</body>
</html>
