@{
    ViewBag.Title = "EXSubmitApproval";
}

@using TECOCITY_BGOI;
<!DOCTYPE html>

<html>
<head>
    <!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN">
    @*<meta name="viewport" content="width=device-width" />*@
    <title>Warn</title>
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery_dialog.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/ui.jqgrid.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.jqGrid.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/My97DatePicker/WdatePicker.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Sales/EXSubmitApproval.js")" type="text/javascript"></script>
    <style>
        .ui-jqgrid tr.jqgrow td {
            white-space: normal !important;
            height: auto;
            vertical-align: text-top;
            padding-top: 2px;
        }
    </style>
</head>

<body>
    @using (Html.BeginForm("SubmitApproval", "SalesManage", FormMethod.Post, new { id = "GForm" }))
    {
        @Html.Hidden("webkey", @ViewData["webkey"])   @Html.Hidden("folderBack", @ViewData["folderBack"])  @Html.Hidden("RelevanceID", @ViewData["RelevanceID"])
        <div id="pageContent">

            <div id="tabTitile"><span style="margin-left:20px;">审批编号：@ViewData["PID"]</span></div>

            <div id="bor" style="width:100%;margin-top:20px;margin-left:15px;">
                <table id="list" class="scroll" style="width:100%;"></table>
                <div id="pager" class="scroll" style="text-align:center;width:100%;"></div>
            </div>
            <div style="margin-top:20px;margin-left:80%"><input class="btn" type="button" id="TJ" value="提交" /></div>
        </div>
    }
</body>
</html>
