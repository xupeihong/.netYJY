@{
    Layout = null;
}
@{
    ViewBag.Title = "";
}
@using TECOCITY_BGOI;
@model  PP_ReceivingInformation
<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Pragma" content="no-cache" />
    <meta http-equiv="Cache-Control" content="no-cache" />
    <meta http-equiv="Expires" content="0" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/PP/ProjectPP/AddFile.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/My97DatePicker/WdatePicker.js")" type="text/javascript" charset=“UTF-8″></script>

</head>
@using (Html.BeginForm("AddFile", "PPManage", FormMethod.Post, new { id = "ProjectformInfo", action = "InsertForm", enctype = "multipart/form-data" }))
{
    <body>
        @Html.Hidden("Hidden", @ViewData["PIDs"])
        @Html.Hidden("msg", @ViewData["msg"])
        <div id="hole" style="overflow-y: auto;">

            <table class="tabInfo" style="width: 97%; margin-left: 8px; margin-top: 10px;">
                <tr>
                    <td>
                        @ViewData["PIDs"]
                        <span id="td1" style="display:none">@ViewData["PIDs"]</span>
                    </td>
                </tr>
                <tr>
                    <td class="textLeft">已存在文件</td>
                    <td colspan="3">
                        <div id="unit"></div>
                    </td>
                </tr>
                <tr>
                    <td class="textLeft">选择上传文件</td>
                    <td class="textright" colspan="3">
                        <input name="file" type="file" id="UploadFile" class="file" /><span style="color: red;">*</span>
                        <div id="Progress" runat="server" style="float: right; width: 85%; color: Green; height: 24px; line-height: 24px;">
                        </div>
                        <div class="foot">
                            <input type="button" id="upLoad" name="upLoad" value="上 传" class="btn" />
                        </div>
                    </td>
                </tr>
            </table>


        </div>
    </body>

}
</html>
