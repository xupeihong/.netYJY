@{
    ViewBag.Title = "";
}

@{
    Layout = null;
}
@using TECOCITY_BGOI;
@model  PP_ReceivingInformation

<html>
<head>
    <meta http-equiv="Pragma" content="no-cache" />
    <meta http-equiv="Cache-Control" content="no-cache" />
    <meta http-equiv="Expires" content="0" />
    <title>ScrapManagementOut</title>
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/Inventory/AddStockIn.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.jqGrid.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/cmm_ClassControl.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/My97DatePicker/WdatePicker.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/PP/ProjectPP/DetailsSH.js")" type="text/javascript"></script>

</head>

<body>
    <form>
        <div id="createAll" style="background-color: #ffffff; width: 100%; ">
            <div id="biaoti">
                <h1 class="title">收货单详情</h1>
            </div>

        </div>
        <table cellpadding="0" cellspacing="0" class="tabInfo">
            <tr class="PPtr">
                <td colspan="4">   订购物品</td>
            </tr>
            <tr>
                <td>
                    <div style="overflow-y: scroll;max-height:200px;">
                        <table id="myTable" cellpadding="0" cellspacing="0" class="tabInfo">
                            <tr align="center" valign="middle">
                                <th style="width: 5%;" class="th">
                                    序号
                                </th>
                                <th style="width: 10%;" class="th">
                                    物品编号
                                </th>
                                <th style="width: 15%;" class="th">
                                    物品名称
                                </th>
                                <th style="width: 10%;" class="th">
                                    规格型号
                                </th>
                         
                                <th style="width: 5%;" class="th">
                                    单位
                                </th>
                                <th style="width: 5%;" class="th">
                                    数量
                                </th>
                                <th style="width: 10%;" class="th">
                                    供应商
                                </th>
                                <th style="width: 15%;" class="th">
                                    单价
                                </th>
                                <th style="width: 10%;" class="th">
                                    总金额
                                </th>
                            </tr>
                            <tbody id="GXInfo"></tbody>
                        </table>
                    </div>
                </td>
            </tr>


        </table>

        <table cellpadding="0" cellspacing="0" class="tabInfo">
            <tr class="PPtr">
                <td colspan="10">   相关约定</td>
            </tr>
            <tr>
                <td>
                    到货日期
                </td>
                <td colspan="3" id="ArrivalDate"></td>
                <td>
                    收货说明
                </td>
                <td colspan="3" id="ArrivalDescription"></td>
            </tr>
        </table>
        <div style="text-align: left; width: 100%; height: 32px; margin-bottom:10px; ">

        </div>
        <div style="width: 100%; height: 70px;">
            <table cellpadding="0" cellspacing="0" class="tabInfo" style="margin-top:5px;">
                <tr class="PPtr">
                    <td colspan="4">   收货人信息</td>
                </tr>
                <tr>
                    <td class="textleft" width="20%">
                        到货处理人
                    </td>
                    <td class="textright" colspan="3" id="ArrivalProcess"></td>
                </tr>

            </table>
        </div>
    </form>
</body>
</html>
