@{
    Layout = null;
}
@using TECOCITY_BGOI;
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>PrintXSContract</title>
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <link href="@Url.Content("~/CSS/Print.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/cmm_PrintPageSet.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/CustomerServiceManage/PrintXSContract.js")" type="text/javascript"></script>
</head>
<body style="margin: 0 0 0 0; background-color: white; overflow: auto;">
    <div id="ReportContent" style="margin-top: 10px; page-break-after: always;">
        <div style="width: 100%; text-align: center; font-size: 22px;font-weight:bold;">合同/标书审批表</div>
        <div style="height: 30px; width: 100%; margin-top: 25px; margin-left: 10px">
            <table class="tabInfo2" style="text-align: left; border: none;">
                <tr style="border: none">
                    <td style="border: none;font-size: 15px;">合同评审&nbsp;口</td>
                    <td style="border: none;font-size: 15px;">标书评审&nbsp;口</td>
                    <td colspan="2" style="border: none;font-size: 15px;">更改评审&nbsp;口</td>
                </tr>
            </table>
        </div>
        <table id="search" class="tabInfo2">
            <tr>
                <td style="width:50px">合同名称或用户名称 </td>
                <td colspan="3">@ViewData["Cname"]</td>
            </tr>
            <tr>
                <td style="width:50px">合同编号</td>
                <td style="width:50px">@ViewData["CID"]</td>
                <td style="width:50px">签署人</td>
                <td style="width:50px"></td>
            </tr>
            <tr>
                <td style="width:50px">合同额</td>
                <td colspan="3"></td>
            </tr>
            <tr>
                <td style="width:50px">成本</td>
                <td colspan="3"></td>
            </tr>
            <tr>
                <td style="width:50px">毛利 </td>
                <td colspan="3"></td>
            </tr>
            <tr>
                <td rowspan="4" style="width:50px">评审结论</td>

            </tr>
            <tr style="height: 200px;">
                <td colspan="3">
                    <div style="height:180px;left:0px;text-align:left;">@ViewData["Remarkfj"]</div>
                    <div style="position: absolute; bottom: 220px;left:230px;">部门经理：@ViewData["namefj"] 
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                        日期：@ViewData["ApprovalTimefj"]</div>
                </td>
            </tr>
            <tr style="height: 200px;">
                <td colspan="3">
                    <div style="height:180px;left:0px;text-align:left;">@ViewData["Remarkf"]</div>
                    <div style="position: absolute; bottom: 20px;left:230px;">公司副经理：@ViewData["namef"] &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;日期：@ViewData["ApprovalTimef"]</div>
                </td>
            </tr>
            <tr style="height: 200px;">
                <td colspan="3">
                    <div style="height:180px;left:0px;text-align:left;">@ViewData["Remark"]</div>
                    <div style="position: absolute; bottom: -180px;left:230px;">公司总经理：@ViewData["name"]   &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;日期：@ViewData["ApprovalTime"]</div>
                </td>
            </tr>
        </table>
        <div style="height: 30px; width: 100%; margin-top: 25px; margin-left: 10px">
            <table class="tabInfo2" style="text-align: left; border: none;">
                <tr style="border: none">
                    <td style="border: none; font-size: 11px;" colspan="4">说明：本表适用于燕山售后服务部，十万元以下合同公司副总经理签字即可，十万元及以上合同需由公司总经理签字</td>
                </tr>
            </table>
        </div>

        <div style="text-align: right; margin-top: 5px; width: 99%;" id="dy">
            <input type="button" id="btnPrint" value="打 印" class="btn" />
        </div>
    </div>

</body>
</html>
