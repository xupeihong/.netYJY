,@{
    Layout = null;
}
@using TECOCITY_BGOI;
@model  tk_SHReturnVisit
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>PrintUserVisit</title>

    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <link href="@Url.Content("~/CSS/Print.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/cmm_PrintPageSet.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/CustomerServiceManage/PrintUserVisit.js")" type="text/javascript"></script>
</head>
<body style="margin: 0 0 0 0; background-color: white; overflow: auto;">
    @* <div id="pageContent" style=" margin-left:2px; margin-right:2px; margin-top:2px; ">*@
    <div id="ReportContent" style="margin-top: 10px; page-break-after: always;">
        <div style="width: 100%; text-align: center; font-size: 18px;">北京市燕山工业燃气设备有限公司</div>
        <div style="font-size: 22px; font-weight: bold; float: left; margin-left: 45%;">用户回访</div>
        <div style="margin-left: 65%; float: left;">回访编号 @Model.HFID</div>
        <div style="margin-left: 65%; float: left;">编号 @Model.RecordID</div>
        <table class="tabInfo2" style="text-align: left;">
            <tr>
                <td colspan="3">日期</td>
               @* <td>产品名称</td>
                <td>产品编号</td>*@
                <td>用户情况简述</td>
                <td>联系人</td>
                <td>电话</td>
                <td>对此次服务满意度</td>
            </tr>
            <tr>
                <td colspan="3">@Model.RVDate</td>
               @* <td>@ViewData["ProdectName"]</td>
                <td>@Model.ProductID</td>*@
                <td>@Model.UserInformation</td>
                <td>@Model.ContactPerson</td>
                <td>@Model.Tel</td>
                <td>@Model.SatisfiedDegree</td>
            </tr>
            @* <tr>
                <td>创建时间</td>
                <td>@Model.CreateTime</td>
                <td>备注</td>
                <td colspan="7">@Model.Remark</td>
            </tr>*@
            <tr>
                <td colspan="7">产品信息</td>
            </tr>
            <tr align="center" valign="middle">
                <td style="width: 20%;">产品编码
                </td>
                <td colspan="2" style="text-align: center;">产品名称
                </td>
                <td colspan="2" style="text-align: center;">规格型号
                </td>
                <td>数量
                </td>
                <td>单价
                </td>
            </tr>
            <tbody id="DetailInfo" class="tabInfo2"></tbody>
            <tr style="height: 30px;">
                <td>联系电话</td>
                <td style="text-align: left;"></td>
                <td>签字</td>
                <td colspan="7" style="text-align: left;"></td>

            </tr>
            <tr>
                <td>日期</td>
                <td colspan="10" style="text-align: left;"></td>
            </tr>
        </table>
        <div style="text-align: right; margin-top: 5px; width: 99%;">
            <input type="button" id="btnPrint" value="打 印" class="btn" />
        </div>
    </div>
    @*</div>*@
</body>
</html>
