@{
    Layout = null;
}
@using TECOCITY_BGOI;
@model tk_PressureAdjustingInspection
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>PrintPressureAdjustingInspection</title>
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <link href="@Url.Content("~/CSS/Print.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/cmm_PrintPageSet.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/CustomerServiceManage/PrintPressureAdjustingInspection.js")" type="text/javascript"></script>
</head>
<body style="margin: 0 0 0 0; background-color: white; overflow: auto;">
    @* <div id="pageContent" style=" margin-left:2px; margin-right:2px; margin-top:2px; ">*@

    <div id="ReportContent" style="margin-top: 10px; page-break-after: always;">
        <div style="width: 100%; text-align: center; font-size: 18px;">北京市燕山工业燃气设备有限公司</div>
        <div style="font-size: 22px; font-weight: bold; float: left; margin-left: 35%;">调压巡检记录表</div>
        <div style="margin-left: 65%; float: left;">调压巡检编号 @Model.TYID</div>
       <table id="search" class="tabInfo2">
            <tr>
                <td>用户名称</td>
                <td class="textRight" colspan="8">@Model.UserName@Html.ValidationMessageFor(model => model.UserName)</td>
            </tr>
            <tr>
                <td>用户地址</td>
                <td class="textRight" colspan="8">@Model.UserAdd@Html.ValidationMessageFor(model => model.UserAdd)</td>
            </tr>
            <tr>
                <td>联系人</td>
                <td class="textRight" colspan="2">@Model.Users@Html.ValidationMessageFor(model => model.Users)</td>
                <td>电话</td>
                <td class="textRight" colspan="5">@Model.Tel@Html.ValidationMessageFor(model => model.Tel)</td>
            </tr>
            <tr style="display:none">
                <td>钥匙保存单位</td>
                <td>甲方</td>
                <td class="textRight" colspan="3">@Model.KeyStorageUnitJia@Html.ValidationMessageFor(model => model.KeyStorageUnitJia)</td>
                <td>已方</td>
                <td class="textRight" colspan="3">@Model.KeyStorageUnitYi@Html.ValidationMessageFor(model => model.KeyStorageUnitYi)</td>
            </tr>
           @* <tr>
                <td>用途 @Model.Uses@Html.ValidationMessageFor(model => model.Uses)</td>
                <td>锅炉</td>
                <td class="textRight">@Model.Boiler@Html.ValidationMessageFor(model => model.Boiler)</td>
                <td>公福</td>
                <td class="textRight">@Model.KungFu@Html.ValidationMessageFor(model => model.KungFu)</td>
                <td>民用</td>
                <td class="textRight">@Model.Civil@Html.ValidationMessageFor(model => model.Civil)</td>
                <td>其它</td>
                <td class="textRight">@Model.Other@Html.ValidationMessageFor(model => model.Civil)</td>
            </tr>*@

                <tr>
                    <td>用途</td>
                    <td colspan="8">
                        @Html.RadioButton("Uses", "0", new { @disabled="false"})锅炉 &nbsp;&nbsp;&nbsp;&nbsp;&nbsp; 
                        @Html.RadioButton("Uses", "1", new { @disabled="false"})公福 &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                        @Html.RadioButton("Uses", "2", new { @disabled="false"})民用 &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                        @Html.RadioButton("Uses", "3", new { @disabled="false"})其它 &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                       
                    </td>
                </tr>
            <tr>
                <td colspan="9">使用压力</td>
            </tr>
            <tr>
                <td colspan="9">
                    <div style="overflow-y: scroll;">
                        <table cellpadding="0" cellspacing="0" class="tabInfo2">
                            <tbody id="DetailInfo"></tbody>
                        </table>
                    </div>
                </td>
            </tr>
            <tr>
                <td>运维时间</td>
                <td class="textRight" colspan="8">
                    @ViewData["OperationTime"]</td>
                @*  <td colspan="2">下午</td>
              <td class="textRight" colspan="2">
                    @Model.AfternoonTime</td>*@
            </tr>
            <tr>
                <td>巡检人员</td>
                <td class="textRight" colspan="8">@Model.InspectionPersonnel@Html.ValidationMessageFor(model => model.InspectionPersonnel)</td>
            </tr>
            <tr>
                <td>用户签字</td>@*@Html.TextBox("UserSignature", "", new { @class = "txtCss" })@Html.ValidationMessageFor(model => model.UserSignature)*@
                <td class="textRight" colspan="8"></td>
            </tr>
            <tr>
                <td>备注</td>
                <td class="textRight" colspan="8">@Model.Remarks@Html.ValidationMessageFor(model => model.Remarks)</td>
            </tr>
            <tr style="height: 30px;">
                <td>联系电话</td>
                <td style="text-align: left;" colspan="2"></td>
                <td>签字</td>
                <td style="text-align: left;" colspan="5"></td>
            </tr>
            <tr>
                <td>日期</td>
                <td colspan="8" style="text-align: left;"></td>
            </tr>
        </table>
        <div style="text-align: right; margin-top: 5px; width: 99%;">
            <input type="button" id="btnPrint" value="打 印" class="btn" />
        </div>
    </div>


    @*</div>*@
</body>
</html>
