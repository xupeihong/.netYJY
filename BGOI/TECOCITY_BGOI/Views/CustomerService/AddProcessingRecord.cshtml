@{
    Layout = null;
}

@using TECOCITY_BGOI
@model tk_SHComplain_Process
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>AddProcessingRecord</title>
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/Sales/RecordInfo.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery_dialog.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />

    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")"  type="text/javascript"></script>
@*    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Validate/Validate-Injection.js")" type="text/javascript"></script>*@

    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.jqGrid.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/cmm_Tables.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/cmm_ClassControl.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/My97DatePicker/WdatePicker.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/CustomerServiceManage/AddProcessingRecord.js")" type="text/javascript"></script>

</head>
<body>
    <form>
        <div id="tabTitile"><span style="margin-left: 10px; float: left;">编号： @ViewData["TSID"]</span></div>@Html.Hidden("TSID", ViewData["TSID"])
        <div style="width:800px;height:20px;float: left;" id="shijian">
            <span style="margin-left: 480px; float: left;">登录日期:</span><div id="localtime" style="float: right;"></div>
        </div>
        <br />
        <div style="height: 300px; overflow-y: auto; text-align: left;">
            <table class="tabInfo" style="width: 99%; margin-left: 8px; margin-top: 10px;">
                <tr style="background-color: #88c9e9; text-align: left;">
                    <td colspan="4">处理信息</td>
                </tr>
                <tr>
                    <td class="textLeft">处理结果 </td>
                    <td>
                        <select id="HandleState">
                            <option value="0">未处理</option>
                            <option value="1">处理中</option>
                            <option value="2">已处理</option>
                        </select>
                    </td>
                    <td>处理时间</td>
                    <td>
                        <input type="text" id="HandleDate" onclick="WdatePicker()" class="Wdate" style="width: 170px;" />
                    </td>
                </tr>
                <tr>
                    <td class="textLeft">客户反馈</td>
                    <td class="textRight" colspan="3">@Html.TextAreaFor(model => model.CustomerFeedback, new { @class = "txtCss", @style = "width:720px;height:50px;"  })@Html.ValidationMessageFor(model => model.CustomerFeedback)</td>
                </tr>
                 <tr>
                    <td class="textLeft">备注</td>
                    <td class="textRight" colspan="3">@Html.TextAreaFor(model => model.HandleProcess, new { @class = "txtCss", @style = "width:720px;height:50px;" })@Html.ValidationMessageFor(model => model.HandleProcess)</td>
                </tr>
            </table>
            <table class="tabInfo" style="width: 99%; margin-left: 8px; margin-top: 10px;">
                <tr style="background-color: #88c9e9; text-align: left;">

                    <td colspan="4">其他信息</td>
                </tr>
                <tr>
                    <td class="textLeft">处理人</td>
                    <td class="textRight">@Html.TextBox("HandleUser", "", new { @class = "txtCss" })</td>
                    <td class="textLeft">登记人</td>
                    <td class="textRight">@Html.TextBoxFor(model => model.CreateUser, new { @class = "txtCss", @readonly = "true" })</td>
                </tr>
              
            </table>
        </div>
        <div style="margin-top: 5px; margin-left: 75%;">
            
            <input class="btn" type="button" id="btnSave" value="保存" />
            <input class="btn" type="button" id="Cancel" value="取消" />
           @* <input class="btn" type="button" id="ShenPi" style="display:none" value="审批人" />*@
            <input type="text" id="DID" style="display: none;" />
        </div>
    </form>
</body>
</html>
