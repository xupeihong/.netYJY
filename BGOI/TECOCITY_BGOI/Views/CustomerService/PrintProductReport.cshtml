@{
    Layout = null;
}
@using TECOCITY_BGOI;
@model  tk_SHInstallR
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>PrintProductReport</title>

    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <link href="@Url.Content("~/CSS/Print.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/cmm_PrintPageSet.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/CustomerServiceManage/PrintProductReport.js")" type="text/javascript"></script>
</head>
<body style="margin: 0 0 0 0; background-color: white; overflow: auto;">
    @* <div id="pageContent" style=" margin-left:2px; margin-right:2px; margin-top:2px; ">*@
    <div id="ReportContent" style="margin-top: 10px; page-break-after: always;">
        <div style="width: 100%; text-align: center; font-size: 18px;">北京市燕山工业燃气设备有限公司</div>
        <div style="font-size: 22px; font-weight: bold; float: left; margin-left: 45%;">产品报装</div>
        <div style="margin-left: 65%; float: left;">报装编号 @Model.BZID</div>
        <table class="tabInfo2" style="width: 99%; margin-left: 8px; margin-top: 10px;">
            <tr >
                 <td colspan="4" id="badj">报装登记</td>
                  <td colspan="4" id="pgd" style="display:none">派工单</td>
            </tr>
            <tr id="idbh" style="display: none">
                <td colspan="4">关联编号 @Html.TextBox("TRID", "", new { @class = "txtCss", @readonly = "true" })
                </td>
            </tr>
            <tr>
                <td colspan="4" style="display: none" id="pgd1">调试信息</td>
                <td colspan="4" id="bzdj1">报装信息</td>
            </tr>
            <tr>
                <td>用户姓名</td>
                <td class="textRight">@Model.CustomerName</td>
                <td>联系电话</td>
                <td class="textRight">@Model.Tel</td>
            </tr>
            <tr>
                <td>地址</td>
                <td class="textRight" colspan="3">@Model.Address</td>
            </tr>
            <tr>
                 <td  id="bzdj2">报装日期</td>
                    <td  style="display: none" id="pgd2">调试日期</td>
                <td class="textRight" colspan="3">@ViewData["InstallTime"] </td>
            </tr>
            <tr>
                <td colspan="4">
                    <div style="overflow-y: scroll;">
                        <table id="myTable" cellpadding="0" cellspacing="0" class="tabInfo2">
                            <tr align="center" valign="middle">
                                <td style="width: 5%;">序号
                                </td>
                                <td style="width: 10%;">产品名称
                                </td>
                                <td style="width: 20%;"">规格型号
                                </td>
                                <td style="width: 5%;">单位
                                </td>
                                <td style="width: 5%;">数量
                                </td>
                                <td style="width: 5%;">单价
                                </td>
                                <td style="width: 15%;">安装价格
                                </td>
                                <td style="width: 20%;">销售渠道
                                </td>
                                <td style="width: 15%;">是否待收款
                                </td>
                               @* <td style="width: 10%;">操作
                                </td>*@
                            </tr>
                            <tbody id="DetailInfo"></tbody>
                        </table>
                    </div>

                </td>
            </tr>
            <tr>
                <td colspan="4" id="bxk" style="display: none">
                    <div style="overflow-y: scroll;">
                        <table id="myTable1" cellpadding="0" cellspacing="0" class="tabInfo2">
                            <tr align="center" valign="middle">
                                <td style="width: 5%;">序号
                                </td>
                                <td style="width: 10%;">产品名称
                                </td>
                                <td style="width: 20%;">保修卡编号
                                </td>
                            </tr>
                            <tbody id="DetailInfoi"></tbody>
                        </table>
                    </div>

                </td>
            </tr>
            <tr></tr>

        </table>
        <table class="tabInfo2" style="width: 99%; margin-left: 8px; margin-top: 10px;" id="bzpg">
            <tr>

                <td colspan="4">报装派工</td>
            </tr>
            <tr>
                <td>出库二级库房</td>
                <td class="textRight" colspan="3">@Model.WarehouseTwo</td>

            </tr>
            <tr>
                <td>是否一级库调拨</td>
                <td>
                    @Model.IsWhether
                </td>
                <td id="OneHouse">一级库调拨</td>
                <td class="textRight">@Model.WarehouseOne</td>
            </tr>
        </table>
        <table class="tabInfo2" style="width: 99%; margin-left: 8px; margin-top: 10px;">
            <tr>
                <td colspan="4">其他信息</td>
            </tr>
            <tr>
                <td>备注</td>
                <td class="textRight" colspan="3">@Model.Remark</td>
            </tr>
             <tr style="height: 30px;" id="jc">
                <td>签字</td>
                <td style="text-align: left;"></td>
                <td>联系电话</td>
                <td  style="text-align: left;"></td>
            </tr>
            <tr id="rq">
                <td>日期</td>
                <td colspan="3" style="text-align: left;"></td>
            </tr>
        </table>
        <div style="text-align: right; margin-top: 5px; width: 99%;" id="dayin">
            <input type="button" id="btnPrint" value="打 印" class="btn" />
        </div>
    </div>
    @*</div>*@
</body>
</html>
