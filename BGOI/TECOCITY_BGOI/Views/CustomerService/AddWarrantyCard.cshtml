@{
    ViewBag.Title = "";
}

@{
    Layout = null;
}
@using TECOCITY_BGOI;
@model tk_WXGuaranteeCard

<html>
<head>
    <meta http-equiv="Pragma" content="no-cache" />
    <meta http-equiv="Cache-Control" content="no-cache" />
    <meta http-equiv="Expires" content="0" />
    <title>保修卡登记</title>
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/Inventory/AddStockIn.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />

    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")"  type="text/javascript"></script>
    @*    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Validate/Validate-Injection.js")" type="text/javascript"></script>*@

    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.jqGrid.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/cmm_ClassControl.js")" type="text/javascript" ></script>
    <script src="@Url.Content("~/Scripts/My97DatePicker/WdatePicker.js")" type="text/javascript" ></script>
    <script src="@Url.Content("~/Scripts/CustomerServiceManage/AddWarrantyCard.js")" type="text/javascript"></script>

</head>

<body>
    <form>
        <div id="tabTitile"><span style="margin-left: 10px; float: left;">编码：@Model.BXKID </span></div>@Html.HiddenFor(model => model.BXKID)

        <div style="width: 800px; height: 20px; float: left;" id="shijian">
            <span style="margin-left: 480px; float: left;">登录日期:</span><div id="localtime" style="float: right;"></div>
        </div>
        <div style="width: 100%; text-align: left;">
            <table class="tabInfo" style="width: 97%; margin-left: 8px; margin-top: 10px;">
                <tr style="text-align: center; font: bold; margin-top: 20px; font-size: 24px;">
                    <td colspan="4">保修卡登记</td>
                </tr>
                <tr>
                    <td class="textLeft">保修卡编号</td>
                    <td class="textRight">@Html.TextBox("BXKNum", "", new { @class = "txtCss" })@Html.ValidationMessageFor(model => model.BXKNum)</td>
                    <td class="textLeft">合同编号</td>
                    <td class="textRight">@Html.TextBox("ContractID", "", new { @class = "txtCss" })@Html.ValidationMessageFor(model => model.ContractID)</td>
                </tr>
                <tr>
                    <td class="textLeft">客户地址</td>
                    <td class="textRight" colspan="3">@Html.TextBox("UserAdd", "", new { @class = "txtCss", @style = "width: 675px;" })@Html.ValidationMessageFor(model => model.UserAdd)
                    </td>
                </tr>
                <tr>
                    <td class="textLeft">产品名称</td>
                    @*  <td class="textRight">@Html.DropDownListFor(model => model.OrderContent, CustomerServiceMan.GetOrderContent(), new { @class = "txtCss", @onchange = "GaiBian()" })</td>*@
                    <td class="textRight">@Html.TextBox("OrderContent", "", new { @class = "txtCss" })
                    <td class="textLeft">产品规格型号</td>
                    @* <td class="textRight">@Html.TextBox("SpecsModels", "", new { @class = "txtCss", @readonly = "true" })*@
                    <td class="textRight">@Html.TextBox("SpecsModels", "", new { @class = "txtCss" })
                        @* @Html.DropDownListFor(model => model.SpecsModels, CustomerServiceMan.GetSpecsModels(), new { @class = "txtCss" })*@</td>
                </tr>
                <tr>
                    <td class="textLeft">产品编号</td>
                    @*<td class="textRight" colspan="3">@Html.TextBox("PID", "", new { @class = "txtCss", @style = "width: 675px;", @readonly = "true" })</td>*@
                    <td class="textRight" colspan="3">@Html.TextBox("PID", "", new { @class = "txtCss", @style = "width: 675px;" })</td>
                </tr>
                <tr>
                    <td class="textLeft">购买日期</td>
                    <td class="textRight">
                        <input type="text" id="BuyDate" onclick="WdatePicker()" class="Wdate" style="width: 170px;" /></td>
                    <td class="textLeft">保修时间</td>
                    <td class="textRight">
                        <input type="text" id="BXDate" onclick="WdatePicker()" class="Wdate" style="width: 170px;" /></td>
                </tr>
                <tr>
                    <td class="textLeft">最终用户单位</td>
                    <td class="textRight" colspan="3">@Html.TextBox("EndUnit", "", new { @class = "txtCss", @style = "width: 675px;" })@Html.ValidationMessageFor(model => model.EndUnit)
                    </td>
                </tr>
                <tr>
                    <td class="textLeft">联系人</td>
                    <td class="textRight">@Html.TextBox("Contact", "", new { @class = "txtCss" })@Html.ValidationMessageFor(model => model.Contact)</td>
                    <td class="textLeft">联系方式</td>
                    <td class="textRight">@Html.TextBox("Tel", "", new { @class = "txtCss" })@Html.ValidationMessageFor(model => model.Tel)</td>
                </tr>
                <tr>
                    <td class="textLeft">备注</td>
                    <td class="textRight" colspan="5">@Html.TextAreaFor(model => model.Remark, new { @class = "txtCss", @style = "width:720px;height:50px;" })@Html.ValidationMessageFor(model => model.Remark)</td>
                </tr>
                <tr>
                    <td class="textLeft">登记人</td>
                    <td class="textRight" colspan="5">@Html.TextBoxFor(model => model.CreateUser, new { @class = "txtCss", @readonly = "true" })</td>
                </tr>
            </table>
            <input type="text" id="DID" style="display: none;" />
        </div>
        <div style="margin-top: 10px; margin-left: 75%">

            <input class="btn" type="button" id="btnSave" value="保存" />
            <input class="btn" type="button" id="Cancel" value="取消" />
        </div>
    </form>
</body>
</html>
