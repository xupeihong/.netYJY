@{
    Layout = null;
}
@using TECOCITY_BGOI;
@model  tk_WXRequisit
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>PrintMaintenanceTaskList</title>

    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <link href="@Url.Content("~/CSS/Print.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/cmm_PrintPageSet.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/CustomerServiceManage/PrintMaintenanceTaskList.js")" type="text/javascript"></script>
</head>
<body style="margin: 0 0 0 0; background-color: white; overflow: auto;">
    @* <div id="pageContent" style=" margin-left:2px; margin-right:2px; margin-top:2px; ">*@
    <div id="ReportContent" style="margin-top: 10px; page-break-after: always;">
        <div style="width: 100%; text-align: center; font-size: 18px;">北京市燕山工业燃气设备有限公司</div>
        <div style="font-size: 22px; font-weight: bold; float: left; margin-left: 35%;">维修记录表</div>
        <div style="margin-left: 70%; float: left;">报修编号 @Model.BXID</div>
        @*  <div style="margin-left: 73%; float: left;">客户名称 @Model.Customer</div>*@
        <table id="search" class="tabInfo2">
            <tr>
                <td>日期</td>
                <td>用户名称</td>
                <td>联系人</td>
                <td>用户地址</td>
                <td>电话</td>
                <td>设备型号</td>

                <td>启用日期</td>
                <td>保修期</td>
                <td>报修日期</td>
                <td>维修日期</td>
                <td>保修卡编号</td>
                <td>维修车辆及人员</td>
                <td>用户报修简述</td>
                <td>维修情况记录</td>
                @*<td>更换零件</td>*@
                <td>维修成本</td>
            </tr>
            <tr>
                <td></td>
                <td>@Model.Customer</td>
                <td>@Model.ContactName</td>
                <td>@Model.Address</td>
                <td>@Model.Tel</td>
                <td>@Model.DeviceType</td>

                <td>@ViewData["EnableDate"]</td>

                <td>@Model.GuaranteePeriod</td>
                <td>@ViewData["RepairDate"]</td>

                <td>@ViewData["MaintenanceTime"]</td>

                <td>@Model.BXKNum</td>
                <td>@ViewData["MaintenanceVehicle"],@ViewData["MaintenanceName"]</td>
                <td>@Model.RepairTheUser</td>
                <td>@ViewData["MaintenanceRecord"]</td>
                @*<td></td>*@
                <td></td>
            </tr>
            <tr >
                <td>备注</td>
                <td colspan="15">@Model.Remark</td>
            </tr>
            <tr style="height: 30px;" id="jc">
                <td>联系电话</td>
                <td style="text-align: left;" colspan="2"></td>
                <td>签字</td>
                <td colspan="11" style="text-align: left;"></td>
            </tr>
            <tr id="rq">
                <td>日期</td>
                <td colspan="15" style="text-align: left;"></td>
            </tr>
        </table>
        <div style="text-align: right; margin-top: 5px; width: 99%;" id="dayin">
            <input type="button" id="btnPrint" value="打 印" class="btn" />
        </div>
    </div>
    @*</div>*@
</body>
</html>
