@{
    Layout = null;
}
@using TECOCITY_BGOI;
@model  tk_SHSurvey
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>PrintUserVisit</title>

    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <link href="@Url.Content("~/CSS/Print.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/cmm_PrintPageSet.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/CustomerServiceManage/PrintCustomerSatisfactionSurvey.js")" type="text/javascript"></script>
</head>
<body style="margin: 0 0 0 0; background-color: white; overflow: auto;">
    @* <div id="pageContent" style=" margin-left:2px; margin-right:2px; margin-top:2px; ">*@
    <div id="ReportContent" style="margin-top: 10px; page-break-after: always;">
        <div style="width: 100%; text-align: center; font-size: 18px;">北京市燕山工业燃气设备有限公司</div>
        <div style="font-size: 22px; font-weight: bold; float: left; margin-left: 35%;">顾客满意度调查表</div>
        <div style="margin-left: 73%; float: left;">顾客满意度编号 @Model.DCID</div>
        @* <div style="margin-left: 73%; float: left;">客户名称 @Model.Customer</div>*@
        <table id="search" class="tabInfo2">
            <tr>
                <td>用户名称</td>
                <td>@Model.Customer</td>
                <td>地址</td>
                <td colspan="3">@Model.Address</td>
            </tr>
            <tr>
                <td>电话/传真</td>
                <td>@Model.Tel</td>
                <td>联系人</td>
                <td colspan="3">@Model.UserPerson</td>
            </tr>
            <tr align="center" valign="middle">
                <td style="width: 15%;">产品编号
                </td>
                <td style="width: 15%;">产品名称
                </td>
                <td style="width: 15%;">规格型号
                </td>
                <td style="width: 15%;">订购方式
                </td>
                <td style="width: 15%;">数量
                </td>
                <td style="width: 15%;">订购时间
                </td>
                @* <th style="width: 15%;" class="th">操作
                </th>*@
            </tr>
            <tbody id="DetailInfo" class="tabInfoP"></tbody>
            <tr>
                <td class="textLeft" colspan="6">您对我公司的产品：</td>
            </tr>
            <tr>
                <td>质量：</td>
                <td colspan="5">
                    @Html.RadioButton("ProductQuality", "0", new { @disabled="false"})非常满意  
                     @Html.RadioButton("ProductQuality", "1", new { @disabled="false"})满意
                     @Html.RadioButton("ProductQuality", "2", new { @disabled="false"})一般  
                     @Html.RadioButton("ProductQuality", "3", new { @disabled="false"})不满意
                </td>
            </tr>
            <tr>
                <td>价格：</td>
                <td colspan="5">
                    @Html.RadioButton("ProductQrice", "0", new { @disabled="false"})非常满意  
                     @Html.RadioButton("ProductQrice", "1", new { @disabled="false"})满意
                     @Html.RadioButton("ProductQrice", "2", new { @disabled="false"})一般  
                     @Html.RadioButton("ProductQrice", "3", new { @disabled="false"})不满意
                </td>
            </tr>
            <tr>
                <td>交货期：</td>
                <td colspan="5">
                    @Html.RadioButton("ProductDelivery", "0", new { @disabled="false"})非常满意  
                     @Html.RadioButton("ProductDelivery", "1", new { @disabled="false"})满意
                     @Html.RadioButton("ProductDelivery", "2", new { @disabled="false"})一般  
                     @Html.RadioButton("ProductDelivery", "3", new { @disabled="false"})不满意
                </td>
            </tr>
            <tr>
                <td class="textLeft">说明原因：</td>
                <td colspan="5">@Model.ProductSurvey</td>
            </tr>
            <tr>
                <td class="textLeft" colspan="6">你对我公司的服务：</td>
            </tr>
            <tr>
                <td>售后维修,保养服务：</td>
                <td colspan="5">
                    @Html.RadioButton("CustomerServiceSurvey", "0", new { @disabled="false"})非常满意  
                     @Html.RadioButton("CustomerServiceSurvey", "1", new { @disabled="false"})满意
                     @Html.RadioButton("CustomerServiceSurvey", "2", new { @disabled="false"})一般  
                     @Html.RadioButton("CustomerServiceSurvey", "3", new { @disabled="false"})不满意
                </td>
            </tr>
            <tr>
                <td>备品,备件供应：</td>
                <td colspan="5">
                    @Html.RadioButton("SupplySurvey", "0", new { @disabled="false"})非常满意  
                     @Html.RadioButton("SupplySurvey", "1", new { @disabled="false"})满意
                     @Html.RadioButton("SupplySurvey", "2", new { @disabled="false"})一般  
                     @Html.RadioButton("SupplySurvey", "3", new { @disabled="false"})不满意
                </td>
            </tr>
            <tr>
                <td>有无漏气现象：</td>
                <td colspan="5">
                    @Html.RadioButton("LeakSurvey", "0", new { @disabled="false"})非常满意  
                     @Html.RadioButton("LeakSurvey", "1", new { @disabled="false"})满意
                     @Html.RadioButton("LeakSurvey", "2", new { @disabled="false"})一般  
                     @Html.RadioButton("LeakSurvey", "3", new { @disabled="false"})不满意
                </td>
            </tr>
            <tr>
                <td  class="textLeft">说明原因
                    </td>
                <td colspan="5"> @Model.ServiceSurvey</td>
            </tr>
            <tr>
                <td class="textLeft" colspan="6">你对我公司的代理：</td>
            </tr>
            <tr>
                <td>售后维修，保养服务：</td>
                <td colspan="5">
                    @Html.RadioButton("AgencySales", "0", new { @disabled="false"})非常满意  
                     @Html.RadioButton("AgencySales", "1", new { @disabled="false"})满意
                     @Html.RadioButton("AgencySales", "2", new { @disabled="false"})一般  
                     @Html.RadioButton("AgencySales", "3", new { @disabled="false"})不满意
                </td>
            </tr>
            <tr>
                <td>咨询，维护培训：</td>
                <td colspan="5">
                    @Html.RadioButton("AgencyConsultation", "0", new { @disabled="false"})非常满意  
                     @Html.RadioButton("AgencyConsultation", "1", new { @disabled="false"})满意
                     @Html.RadioButton("AgencyConsultation", "2", new { @disabled="false"})一般  
                     @Html.RadioButton("AgencyConsultation", "3", new { @disabled="false"})不满意
                </td>
            </tr>
            <tr>
                <td>备品，备件供应：</td>
                <td colspan="5">
                    @Html.RadioButton("AgencySpareParts", "0", new { @disabled="false"})非常满意  
                     @Html.RadioButton("AgencySpareParts", "1", new { @disabled="false"})满意
                     @Html.RadioButton("AgencySpareParts", "2", new { @disabled="false"})一般  
                     @Html.RadioButton("AgencySpareParts", "3", new { @disabled="false"})不满意
                </td>
            </tr>
            <tr>
                <td class="textLeft">说明原因</td>
                <td colspan="5"> @Model.AgencySurvey</td>
            </tr>
            <tr>
                <td  class="textLeft">备注 </td>
                <td colspan="5"> @Model.Remark</td>
            </tr>
            <tr style="height: 30px;">
                <td>用户签字</td>
                <td style="text-align: left;"></td>
                <td>联系电话</td>
                <td  colspan="3" style="text-align: left;"></td>
            </tr>
            <tr>
                <td>日期</td>
                <td colspan="5" style="text-align: left;"></td>
            </tr>
        </table>
        <div style="text-align: right; margin-top: 5px; width: 99%;">
            <input type="button" id="btnPrint" value="打 印" class="btn" />
        </div>
    </div>
    @*</div>*@
</body>
</html>
