@{
    Layout = null;
}
@using TECOCITY_BGOI;
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>PrintXSContract</title>
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <link href="@Url.Content("~/CSS/Print.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/cmm_PrintPageSet.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/CustomerServiceManage/PrintXSContractCG.js")" type="text/javascript"></script>
</head>
<body style="margin: 0 0 0 0; background-color: white; overflow: auto;">
    <div id="ReportContent" style="margin-top: 10px; page-break-after: always;">
        <div style="width: 100%; text-align: center; font-size: 22px;font-weight:bold;">采购或委托合同评审表</div>
        <div style="height: 30px; width: 100%; margin-top: 25px; margin-left: 10px">
            <table class="tabInfo2" style="text-align: left; border: none;">
                <tr style="border: none">
                    <td style="border: none;font-size: 15px;">合同评审&nbsp;口</td>
                    @*<td style="border: none;font-size: 15px;">标书评审&nbsp;口</td>*@
                    <td colspan="3" style="border: none;font-size: 15px;">合同更改评审&nbsp;口</td>
                </tr>
            </table>
        </div>
        <table id="search" class="tabInfo2">
            <tr>
                <td style="width:50px">合同名称或用户名称 </td>
                <td colspan="3">@ViewData["Cname"]</td>
            </tr>
             <tr>
                <td colspan="2" style="text-align:left;">甲方： </td>
                <td colspan="2"  style="text-align:left;">乙方：</td>
            </tr>
            <tr>
                <td style="width:50px">合同编号</td>
                <td style="width:50px">@ViewData["CID"]</td>
                <td style="width:50px">所属项目</td>
                <td style="width:50px">@ViewData["Text"]</td>
            </tr>
            <tr>
                <td style="width:50px">合同额</td>
                <td colspan="3"></td>
            </tr>
            <tr>
                <td style="width:50px">合同类别</td>
                <td colspan="3">
                     @Html.CheckBox("ProductForm", new { @value = "0",@disabled="false" })施工 &nbsp;&nbsp;&nbsp;&nbsp;&nbsp; 
                        @Html.CheckBox("ProductForm", new { @value = "0",@disabled="false" })设备 &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                        @Html.CheckBox("ProductForm", new { @value = "0",@disabled="false" })材料&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                        @Html.CheckBox("ProductForm", new { @value = "0",@disabled="false" })辅材&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                        @Html.CheckBox("ProductForm", new { @value = "0",@disabled="false" })外加工  &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                        @Html.CheckBox("ProductForm", new { @value = "0",@disabled="false" })设计&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                       @Html.CheckBox("ProductForm", new { @value = "0",@disabled="false" })其他___
                </td>
            </tr>
            <tr>
                <td style="width:50px;font-size: 18px;font-weight:bold;" colspan="4">注：施工合同必须签订安全协议 </td>
            </tr>
            <tr style="height: 200px;">
                 <td style="width:50px">部门经理意见</td>
                <td colspan="3">
                    <div style="height:180px;left:0px;text-align:left;">(包括：对乙方基本情况的了解，对方签字人是否有权代理：合同订立的必要性、内容是否与恰谈一致、真实可行；合同金额、支付比例、服务范围、供货周期等约定事项的审核确认，文字检查校对及相关修改建议)<br />@ViewData["Remarkfj"]</div>
                    <div style="position: absolute; bottom: 180px;">项目经理：@ViewData["namefj"] 
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                        日期：@ViewData["ApprovalTimefj"]</div>
                </td>
            </tr>
             <tr style="height: 200px;">
                 <td style="width:50px">技术评审意见</td>
                <td colspan="3">
                    <div style="height:180px;left:0px;text-align:left;">(包括：产品型号、规格、数量、质量是否达到项目要求以及相关技术建议)<br /></div>
                    <div style="position: absolute; bottom: -20px;">总工程师： 
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                        日期：</div>
                </td>
            </tr>
            <tr style="height: 200px;">
                  <td style="width:50px">商务评审意见</td>
                <td colspan="3">
                    <div style="height:180px;left:0px;text-align:left;">（包括：合同价格是否低价合理、付款方式是否接受等价条款审核）<br />@ViewData["Remarkf"]</div>
                    <div style="position: absolute; bottom: -220px;">主管副总经理：@ViewData["namef"] &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;日期：@ViewData["ApprovalTimef"]</div>
                </td>
            </tr>
            <tr style="height: 200px;">
                <td style="width:50px">综合评审意见</td>
                <td colspan="3">
                    <div style="height:180px;left:0px;text-align:left;">（包括：综合上述意见，做出相关决定）<br />@ViewData["Remark"]</div>
                    <div style="position: absolute; bottom: -420px;">总经理：@ViewData["name"]   &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                         &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;日期：@ViewData["ApprovalTime"]</div>
                </td>
            </tr>
        </table>
       @* <div style="height: 30px; width: 100%; margin-top: 25px; margin-left: 10px">
            <table class="tabInfo2" style="text-align: left; border: none;">
                <tr style="border: none">
                    <td style="border: none; font-size: 11px;" colspan="4">说明：本表适用于燕山售后服务部，十万元以下合同公司副总经理签字即可，十万元及以上合同需由公司总经理签字</td>
                </tr>
            </table>
        </div>*@

        <div style="text-align: right; margin-top: 5px; width: 99%;" id="dy">
            <input type="button" id="btnPrint" value="打 印" class="btn" />
        </div>
    </div>

</body>
</html>
