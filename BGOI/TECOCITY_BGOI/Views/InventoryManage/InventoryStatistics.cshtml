@using TECOCITY_BGOI;
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN">
    @*<meta name="viewport" content="width=device-width" />*@
    <title>InventoryStatistics</title>
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery_dialog.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/ui.jqgrid.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.jqGrid.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
     <script src="@Url.Content("~/Scripts/My97DatePicker/WdatePicker.js")" type="text/javascript" ></script>
    <script src="@Url.Content("~/Scripts/InventoryManage/InventoryStatistics.js")" type="text/javascript"></script>
    <style>
        .ui-jqgrid tr.jqgrow td {
            white-space: normal !important;
            height: auto;
            vertical-align: text-top;
            padding-top: 2px;
        }
    </style>
</head>

<body>
    @using (Html.BeginForm("InventoryToExcel", "InventoryManage", FormMethod.Post, new { id = "GForm" }))
    {
        <div id="pageContent">
            <div class="mainHead">
                <img src="../images/house.png" class="house" />&nbsp;&nbsp;&nbsp;&nbsp;当前位置>><span style="color: #000099;">统计汇总</span>>>入库汇总
            </div>
          @*  <div id="search" style="height: 50px;">
                <table class="tabSearch" cellpadding="0" cellspacing="0" style="">
                
                </table>
            </div>*@

            <div style="margin-top: 5px; margin-left: 10px;">
               @* <input class="btn" type="submit" id="DaoOut" value="导出" />*@
                @* <input class="btn" type="button" value="查  询" onclick="Search()" />*@
                <input type="button" class="btn" style="float: right; margin-right: 15px;" id="btnPrint" value="打  印" />
            </div>
            <div id="bor" style="width: 100%; margin-top: 5px; margin-left: 10px;">
                <table id="list" class="scroll" style="width: 100%;">
                    <tr>
                        <td>
                            <div style="overflow-y: scroll; max-height: 400px;">
                                <table id="myTable" cellpadding="0" cellspacing="0" class="tabInfo" border="1">
                                    <tr align="center" valign="middle">
                                        <th style="width: 20%;" class="th">物料编码
                                        </th>
                                        <th style="width: 20%;" class="th">物料名称
                                        </th>
                                        <th style="width: 20%;" class="th">规格型号 @*仓库名称*@
                                        </th>
                                        <th style="width: 10%;" class="th">计量单位
                                        </th>
                                         <th style="width: 10%;" class="th">入库数量  @*库存数量*@
                                        </th>
                                        <th style="width: 10%;" class="th">在线生产数量
                                        </th>
                                       @*  <th style="width: 10%;" class="th">库存成品零件数（套）
                                        </th>*@
                                    </tr>
                                    <tbody id="DetailInfo">
                                    </tbody>
                                </table>
                            </div>
                        </td>
                    </tr>
                </table>
               @* <div id="pager" class="scroll" style="text-align: center; width: 100%;"></div>*@
            </div>
        </div>
    }
</body>
</html>
