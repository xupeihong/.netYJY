@{
    Layout = null;
}
@using TECOCITY_BGOI;
@model  tk_StockRemain
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>PrintInventoryFirstPage</title>

    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <link href="@Url.Content("~/CSS/Print.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/cmm_PrintPageSet.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/InventoryManage/PrintInventoryFirstPage.js")" type="text/javascript"></script>
</head>
<body style="margin: 0 0 0 0; background-color: white; overflow: auto;">
    @* <div id="pageContent" style=" margin-left:2px; margin-right:2px; margin-top:2px; ">*@
    <div id="ReportContent" style="margin-top: 10px; page-break-after: always;">
        <div style="width: 100%; text-align: center; font-size: 18px;">北京市燕山工业燃气设备有限公司</div>
        <div style="font-size: 22px; font-weight: bold; float: left; margin-left: 45%;">货品库存</div>
        <div style="margin-left: 80%; float: left;">货品编号 @ViewData["PID"]</div>
        @*  <div style="margin-left: 73%; float: left;">编号：@Model.RecordID</div>*@
        <table id="search" class="tabInfo2" style="text-align: left;">
            <tr>
                <td style=" width:85px">货品名称:</td>
                <td style=" width:85px">@ViewData["ProName"]</td>
                <td style=" width:85px">规格型号:</td>
                <td style=" width:85px">@ViewData["Spec"]</td>
                <td style=" width:85px">在线生产数量:</td>
                <td style=" width:85px">@Model.OnlineCount</td>
                <td style=" width:85px">所属仓库</td>
                <td style=" width:85px">@ViewData["HouseName"]</td>

            </tr>
            <tr>
                <td style=" width:85px">货品库类型:</td>
                <td style=" width:85px">@ViewData["Text"]</td>
                <td style=" width:85px">存放位置:</td>
                <td style=" width:85px">@Model.Location</td>
                <td style=" width:85px">可用库存:</td>
                <td style=" width:85px">@Model.UsableStock</td>
                <td style=" width:85px">库存数量</td>@*@Model.Costing*@
                <td style=" width:85px">@Model.FinishCount</td>
               @* <td style=" width:60px">成本:</td>@Model.Costing
                <td style=" width:60px"></td>*@
            </tr>





            <tr style="height: 30px;">
                <td  style=" width:85px">联系电话:</td>
                <td style="text-align: left;width:85px"></td>
                <td  style=" width:85px">签字:</td>
                <td colspan="5" style="text-align: left;width:85px"></td>
            </tr>
            <tr>
                <td  style=" width:85px">日期:</td>
                <td colspan="7" style="text-align: left;width:85px"></td>
            </tr>
        </table>
        <div style="text-align: right; margin-top: 5px; width: 99%;">
            <input type="button" id="btnPrint" value="打 印" class="btn" />
        </div>
    </div>
    @*</div>*@
</body>
</html>
