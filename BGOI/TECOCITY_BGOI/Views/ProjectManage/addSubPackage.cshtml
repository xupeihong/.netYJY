
@{
    Layout = null;
}
@using TECOCITY_BGOI;
@model tk_SubPackage


<!DOCTYPE html>

<html>
<head>
    <!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN">
    @*<meta name="viewport" content="width=device-width" />*@
    <title>Warn</title>
    <link href="@Url.Content("~/CSS/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery_dialog.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/ui.jqgrid.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")"  type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Validate/Validate-Injection.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.jqGrid.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/My97DatePicker/WdatePicker.js")" type="text/javascript" ></script>
    <script src="@Url.Content("~/Scripts/ProjectManage/addSubPackage.js")" type="text/javascript"></script>
   <style>
       .ui-jqgrid tr.jqgrow td {
        white-space: normal !important;
        height:auto;
        vertical-align:text-top;
        padding-top:2px;
        }
       </style>
</head>
   
<body>
    @using (Html.BeginForm("addSubPackage", "ProjectManage", FormMethod.Post, new { id = "ProjectformInfo", onsubmit = "return returnConfirm();" }))
    {
        
    <div id="pageContent">
       
    <div id="tabTitile"><span style="margin-left:20px;">项目编号：@Model.StrPID</span></div>@Html.HiddenFor(model => model.StrPID)
        <table class="tabInfo" style="width: 97%;margin-left:10px;margin-top:10px;">
       @* <tr>
            <td class="textLeft" style="width:150px;">分包设计编号</td><td class="textRight">@Html.TextBoxFor(model => model.StrSID, new {@class="txtCss" ,@ReadOnly = "ReadOnly",onkeypress="return !(event.keyCode==8)",onkeydown="return !(event.keyCode==8)"}) </td>
        </tr>*@
         <tr>
            <td class="textLeft">项目名称</td><td class="textRight">@Html.TextBox("Pname",@ViewData["Pname"], new {style="width:90%" ,@ReadOnly = "ReadOnly",onkeypress="return !(event.keyCode==8)",onkeydown="return !(event.keyCode==8)"})</td>
        </tr>
        <tr>
            <td class="textLeft">分包设计单位</td><td class="textRight">@Html.TextBoxFor(model => model.StrDesignUnit, new {@class="txtCss" }) <span style="color:red;"> * </span> @Html.ValidationMessageFor(model => model.StrDesignUnit)</td>
       </tr>
          <tr>
            <td class="textLeft">分包设计单位项目负责人</td><td class="textRight">@Html.TextBoxFor(model => model.StrDesignPrincipal, new {@class="txtCss"}) <span style="color:red;"> * </span> @Html.ValidationMessageFor(model => model.StrDesignPrincipal)</td>
        </tr>
        <tr>
            <td class="textLeft">分包设计费用</td><td class="textRight">@Html.TextBoxFor(model => model.StrDesignPrice, new { @class="txtCss"}) 万元 @Html.ValidationMessageFor(model => model.StrDesignPrice)</td>
        </tr>
         <tr>
            <td class="textLeft">预计设计周期</td><td class="textRight">@Html.TextBoxFor(model => model.StrPredictCycle, new { @class="txtCss"})  @Html.ValidationMessageFor(model => model.StrPredictCycle)</td>
        </tr>
       <tr>
            <td class="textLeft">分包设计原因</td><td class="textRight">@Html.TextAreaFor(model => model.StrSubReason, new { style="width:90%;height:100px;"})  @Html.ValidationMessageFor(model => model.StrSubReason)</td>
        </tr>
        <tr>
            <td class="textLeft">设计主要内容</td><td class="textRight">@Html.TextAreaFor(model => model.StrMainContent, new { style="width:90%;height:100px;"})  @Html.ValidationMessageFor(model => model.StrMainContent)</td>
        </tr>
    </table>
       <div style="margin-top:10px;margin-left:75%">
             <input class="btn" type="submit"  id="charge" value="确定"/>
       </div> 
   
    </div>
    }
</body>
</html>
