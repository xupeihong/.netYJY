
@{
    Layout = null;
}
@using TECOCITY_BGOI;
@model tk_SubWork


<!DOCTYPE html>

<html>
<head>
    <!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN">
    @*<meta name="viewport" content="width=device-width" />*@
    <title>Warn</title>
    <link href="@Url.Content("~/Scripts/uploadify/uploadify.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/uploadify/jquery.uploadify.min.js")"></script>
     @* <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")"  type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Validate/Validate-Injection.js")" type="text/javascript"></script>*@
    <script src="@Url.Content("~/Scripts/jquery.jqGrid.min.js")" type="text/javascript"></script>
    

  
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/My97DatePicker/WdatePicker.js")" type="text/javascript" ></script>
    <script src="@Url.Content("~/Scripts/ProjectManage/addSubWork.js")" type="text/javascript"></script>
   <style>
       .ui-jqgrid tr.jqgrow td {
        white-space: normal !important;
        height:auto;
        vertical-align:text-top;
        padding-top:2px;
        }
       </style>
</head>
   
<body>
    @using (Html.BeginForm("addSubWork", "ProjectManage", FormMethod.Post, new { id = "ProjectformInfo" }))
    {
        
    <div id="pageContent" style="overflow-y:auto;">
       
    <div id="tabTitile"><span style="margin-left:20px;">项目编号：@Model.StrPID</span></div>@Html.HiddenFor(model => model.StrPID)
        <table class="tabInfo" style="width: 97%;margin-left:10px;margin-top:10px;">
         <tr>
            <td class="textLeft"  style="width:20%;">分包施工编号</td><td class="textRight">@Html.TextBoxFor(model => model.StrEID, new {style="width:200px;",@ReadOnly = "ReadOnly",onkeypress="return !(event.keyCode==8)",onkeydown="return !(event.keyCode==8)"})</td>
        </tr>
         <tr>
            <td class="textLeft">项目名称</td><td class="textRight">@Html.TextBox("Pname",@ViewData["Pname"], new {style="width:90%" ,@ReadOnly = "ReadOnly",onkeypress="return !(event.keyCode==8)",onkeydown="return !(event.keyCode==8)"})</td>
        </tr>
        <tr>
            <td class="textLeft">分包单位</td><td class="textRight">@Html.TextBoxFor(model => model.StrSubUnit, new {@class="txtCss" }) <span style="color:red;"> * </span> @Html.ValidationMessageFor(model => model.StrSubUnit)</td>
       </tr>
          <tr>
            <td class="textLeft">分包单位项目负责人</td><td class="textRight">@Html.TextBoxFor(model => model.StrPrincipal, new {@class="txtCss"}) <span style="color:red;"> * </span> @Html.ValidationMessageFor(model => model.StrPrincipal)</td>
        </tr>
        <tr>
            <td class="textLeft">分包费用</td><td class="textRight">@Html.TextBoxFor(model => model.StrSubPrice, new { @class="txtCss"}) 万元 @Html.ValidationMessageFor(model => model.StrSubPrice)</td>
        </tr>
         <tr>
            <td class="textLeft">预计分工施工周期</td><td class="textRight">@Html.TextBoxFor(model => model.StrWorkCycle, new { @class="txtCss"})  @Html.ValidationMessageFor(model => model.StrWorkCycle)</td>
        </tr>
       <tr>
            <td class="textLeft">分包施工原因</td><td class="textRight">@Html.TextAreaFor(model => model.StrSubWorkReason, new { style="width:90%;height:100px;"})  @Html.ValidationMessageFor(model => model.StrSubWorkReason)</td>
        </tr>
        <tr>
            <td class="textLeft">分包施工主要内容</td><td class="textRight">@Html.TextAreaFor(model => model.StrMainContent, new { style="width:90%;height:100px;"})  @Html.ValidationMessageFor(model => model.StrMainContent)</td>
        </tr>
          <tr>
            <td class="textLeft">是否签订安全施工协议</td><td class="textRight">@Html.RadioButton("IsSign",0) 否 @Html.RadioButton("IsSign",1) 是 <span style="color:red;"> * </span> </td> @Html.HiddenFor(model => model.StrIsSign)
        </tr>
      
    </table>
       <div style="margin-top:5px;margin-left:75%">
             <input class="btn" type="button"  id="charge" value="确定"/>
       </div> 
       <table class="tabInfo" style="width: 97%; margin-left: 8px; margin-top: 5px;overflow-y:auto;">
                 <tr style="height:100px;">
                    <td class="textLeft" style="width:20%;">上传文件</td>
                    <td> <input id="uploadify" type="file" name="uploadfy" style="margin-left: 150px;" />
                         <input class="btn" type="button" id="upload" value="上 传" style="margin-left: 74%;display:none;" />
                    </td>
                </tr>
            </table>
    </div>
    }
</body>
</html>
