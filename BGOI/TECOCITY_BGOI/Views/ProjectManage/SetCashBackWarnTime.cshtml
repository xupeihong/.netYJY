</head>
@using (Html.BeginForm("SetCashBackWarnTime", "ProjectManage", FormMethod.Post, new { id = "ProjectformInfo" }))
{
    <body>
        <div id="hole" style="overflow-y: auto;">
            <table class="tabInfo" style="width: 97%; margin-left: 8px; margin-top: 10px;">

                <tr>
                    <td class="textLeft">提醒范围</td>
                    <td class="textRight">@Html.TextBox("num") 月<span style="color: red">*</span></td>
                </tr>

            </table>


            <div style="margin-top: 10px; margin-left: 80%">
                <input class="btn" type="button" id="charge" value="确定" />
            </div>

        </div>
    </body>
       
}
</html>
