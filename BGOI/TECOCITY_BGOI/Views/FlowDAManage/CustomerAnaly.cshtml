@{
    Layout = null;
}
@using TECOCITY_BGOI;

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>CustomerAnaly</title>

    <link href="@Url.Content("~/CSS/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery_dialog.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")"  type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/My97DatePicker/WdatePicker.js")" type="text/javascript" charset="utf-8"></script>
    <script src="@Url.Content("~/Scripts/Table_Fix.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/TableMergeCell.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Columjs/highcharts.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/FlowDAManage/CustomerAnaly.js")" type="text/javascript"></script>
</head>
@{
    Acc_Account account = GAccount.GetAccountInfo();
    int Rights = account.UserID;
}
<body>
    <div id="pageContent" style="overflow-y: scroll;">
        <div class="mainHead">
            <img src="../images/house.png" class="house" />&nbsp;&nbsp;&nbsp;&nbsp;
                当前位置>><span style="color: #000099;">数据分析>>故障点分析</span>>>大客户数据分析
        </div>
        <div style="margin-left: 10px; margin-top: 10px;">
            @if (Rights != 321)
            {  
                <input type="button" class="btnL" value="导出表一" onclick="ToEXCEL('tabCustomer');" />
                <input type="button" class="btnL" value="导出表二" onclick="ToEXCEL('tabCustomer2');" />
            }
        </div>
        <div style="width: 100%; margin-top: 5px;">
            <table id="tabCustomer" align="center" class="tabInfoWork" style="width: 80%;">
                <tr id="line1"></tr>
                <tbody id="tabDetail" class="tabInfoWork"></tbody>
            </table>
        </div>
        <div id="divImg" style="width: 80%;" align="center">
            <div id="divchart" style="margin-left: 10px; width: 70%;">
            </div>
        </div>
        <div style="width: 100%;">
            <table id="tabCustomer2" align="center" class="tabInfoWork" style="width: 70%;">
                <tr id="line2"></tr>
                <tbody id="tabTotal" class="tabInfoWork"></tbody>
            </table>
        </div>
    </div>
</body>
</html>
