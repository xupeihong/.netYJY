@{
    Layout = null;
}
@using TECOCITY_BGOI
@model Tk_SupplierBas
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>YearResult</title>
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/My97DatePicker/WdatePicker.js")" type="text/javascript" charset=“UTF-8″></script>
    <script src="@Url.Content("~/Scripts/SupplyManage/YearResult.js")"></script>
</head>
@using (Html.BeginForm("YearResult", "SuppliesManage", FormMethod.Post, new { id = "YearRes" }))
{
    <body>
        <div id="hole" style="overflow-y: scroll;">
            <div id="tabTitile"><span style="margin-left: 10px;">流水号：@Html.HiddenFor(model => model.Sid)@ViewBag.sid</span></div>
            <table class="tabInfo" style="width: 97%; margin-left: 8px; margin-top: 10px;">
                <tr>
                    <td class="textLeft">处理意见：</td>
                    <td class="textright" colspan="4">
                        @Html.Hidden("Opinions")
                        <input type="checkbox" name="0" class="ckb2" value="0" />
                        停止供货
                        <input type="checkbox" name="1" class="ckb2" value="1" />
                        暂停供货
                       <input type="checkbox" name="2" class="ckb2" value="2" />
                        淘汰该供应商
                         <span style="color: red;">*</span>
                    </td>
                </tr>
                <tr>
                    <td class="textLeft">处理原因：</td>
                    <td class="textRight" colspan="3">@Html.TextArea("OpinionsD", new { @class = "txtCss", maxlength = "200", style = "height:100px;width:750px;" })<span style="color: red;">*</span></td>
                </tr>
                <tr>
                    <td class="textLeft">部门负责人：</td>
                    <td class="textRight">@Html.TextBox("Approval1User", "", new { @class = "txtCss" }) <span style="color: red;">*</span></td>
                    <td class="textLeft">处理日期：</td>
                    <td class="textRight">@Html.TextBox("ApprovalTime1", "", new { onclick = "WdatePicker({dateFmt: 'yyyy-MM-dd' })", @class = "txtCss" })<span style="color: red;">*</span></td>
                </tr>
            </table>
            <div style="margin-left: 80%; margin-top: 5%;">
                <input class="btn" type="button" id="charge" value="确定" />
            </div>
        </div>
    </body>
}
</html>
