@{
    ViewBag.Title = "";
}

@{
    Layout = null;
}
@using TECOCITY_BGOI
@model tk_SFileInfo
<!DOCTYPE html>

<html>
<head>
    @*  <meta name="viewport" content="width=device-width" />*@
    <meta name="renderer" content="webkit|ie-comp|ie-stand">
    <meta http-equiv="Pragma" content="no-cache" />
    <meta http-equiv="Cache-Control" content="no-cache" />
    <meta http-equiv="Expires" content="0" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/Sales/RecordInfo.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")"  type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Validate/Validate-Injection.js")" type="text/javascript"></script>

    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.jqGrid.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/My97DatePicker/WdatePicker.js")" type="text/javascript" charset=“UTF-8″></script>
    <script src="@Url.Content("~/Scripts/SupplyManage/AddFile.js")" type="text/javascript"></script>
</head>
@using (Html.BeginForm("AddFileInfo", "SuppliesManage", FormMethod.Post, new { id = "FileInfo", action = "InsertFileMsg", enctype = "multipart/form-data" }))
{
    <body>
        @Html.Hidden("msg", @ViewData["msg"])
        <div id="tabTitile">流水号:@Html.HiddenFor(model => model.Sid)@ViewBag.sid</div>
        <div id="hole" style="overflow-y: auto;">
            <table class="tabInfo" style="width: 97%; margin-left: 8px; margin-top: 10px;">
                <tr>
                    <td class="textLeft">资质类型</td>
                    <td class="textRight"style="text-align:left;">@Html.DropDownListFor(model => model.Typeo, SupplyManage.GetConfigType("TypeO"), new { @class = "txtCss" }) <span style="color: red;">*</span></td>
                </tr>
                <tr>
                    <td class="textLeft">资质具体项</td>
                    <td class="textRight"style="text-align:left;">@Html.DropDownListFor(model => model.Ftype, SupplyManage.GetConfigType("Ftype"), new { @class = "txtCss", @disabled = "disabled" })  @Html.ValidationMessageFor(model => model.Ftype)</td>
                </tr>
                <tr>
                    <td class="textLeft">资质证书具体项</td>
                    <td class="textRight"style="text-align:left;">
                        @Html.DropDownListFor(model => model.Item, SupplyManage.GetConfigType("Item"), new { @class = "txtCss", @disabled = "disabled" })
                       
                    </td>
                </tr>
                <tr>
                    <td class="textLeft">其他项说明</td>
                    <td class="textRight"style="text-align:left;">@Html.TextBoxFor(model => model.Itemo, new { @class = "txtCss", @disabled = "disabled" })  @Html.ValidationMessageFor(model => model.Itemo)</td>
                </tr>
                <tr>
                    <td class="textLeft">资质到期时间</td>
                    <td class="textRight"style="text-align:left;">@Html.TextBoxFor(model => model.FTimeOut, new { @class = "txtCss", onclick = "WdatePicker({dateFmt: 'yyyy-MM-dd' })", @readonly = true })</td>
                </tr>

                <tr>
                    <td class="textLeft">请上传资质文档</td>
                    <td class="textRight" colspan="3"style="text-align:left;">
                        <input name="file" type="file" multiple="multiple" id="UploadFile" class="file" />
                        <div id="Progress" runat="server" style="float: right; width: 85%; color: Green; height: 24px; line-height: 24px;">
                        </div>
                    </td>
                </tr>
            </table>

            <div style="margin-top: 10px; margin-left:250px;">
                <input type="button" value="确定" id="sure1" class="btn" style="cursor: pointer" />
            </div>
        </div>
    </body>
}

</html>
