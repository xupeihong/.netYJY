@{
    Layout = null;
}
@using TECOCITY_BGOI
@model tk_SUPSugestion

<!DOCTYPE html>

<html>
<head>
    @*<meta name="viewport" content="width=device-width" />*@
    <meta name="renderer" content="webkit|ie-comp|ie-stand">
    <title>BackSugestionApproval</title>
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery_dialog.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/ui.jqgrid.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.jqGrid.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src=" @Url.Content("~/Scripts/cmm_Tables2.js")"></script>
    <script src="@Url.Content("~/Scripts/cmm_ClassControl.js")"></script>
    <script src="@Url.Content("~/Scripts/My97DatePicker/WdatePicker.js")" type="text/javascript" charset=“UTF-8″></script>
    <script src="@Url.Content("~/Scripts/SupplyManage/BackSugestionApproval.js")"></script>
</head>
<body>
    @using (Html.BeginForm("BackSugestionApproval", "SuppliesManage", FormMethod.Post, new { id = "InnerInfo", action = "BackupSugestionApproval", enctype = "multipart/form-data" }))
    {
       
        @Html.Hidden("msg", @ViewData["msg"])
        <div id="tabTitile">&nbsp;&nbsp;&nbsp;流水号:@Html.HiddenFor(model => model.Sid)@ViewBag.sid</div>
        <div id="hole" style="overflow-y: scroll;">
            <table class="tabInfo" style="width: 97%; margin-left: 8px; margin-top: 10px;">
                <tr>
                    <td class="textLeft">处理意见：</td>
                    <td class="textright" colspan="4">
                        @Html.Hidden("Opinions", @ViewData["Opinions"])
                        @Html.RadioButton("Opinions", "0", new { disabled = "disabled" })
                        停止供货
                        @Html.RadioButton("Opinions", "1", new { disabled = "disabled" })
                        暂停供货
                        @Html.RadioButton("Opinions", "2", new { disabled = "disabled" })
                        淘汰该供应商
                    </td>
                </tr>
                <tr>
                    <td class="textLeft">是否同意恢复供应商：</td>
                    <td class="textRight" colspan="3">
                        @Html.RadioButton("SState", "0", new { @checked = true }) 是
                        @Html.RadioButton("SState", "1") 否<span style="color: red;">*</span>
                    </td>
                </tr>
                <tr>
                    <td class="textLeft">恢复供应商评价：</td>
                    <td class="textRight" colspan="3">@Html.TextAreaFor(model => model.SContent, new { @class = "txtApproval" })<span style="color: red;">*</span></td>
                </tr>
                <tr>
                    <td class="textLeft">恢复评价人：</td>
                    <td class="textRight" colspan="3">@Html.TextBoxFor(model => model.Sperson, new { @class = "txtCss" })<span style="color: red;">*</span></td>

                </tr>
                <tr>
                    <td class="textLeft">恢复建议时间：</td>
                    <td class="textRight" colspan="3">@Html.TextBoxFor(model => model.SCreate, new { @class = "txtCss", onclick = "WdatePicker({dateFmt: 'yyyy-MM-dd' })" })<span style="color: red;">*</span></td>
                </tr>
            </table>
            <div style="margin-top: 10px;">
                <table>
                    <tr>
                        <td>
                            <input type="button" value="确定" id="FZSugtion" class="btn" style="cursor: pointer; margin-left: 550px;" /></td>
                    </tr>
                </table>

            </div>
        </div>
    }
</body>
</html>
