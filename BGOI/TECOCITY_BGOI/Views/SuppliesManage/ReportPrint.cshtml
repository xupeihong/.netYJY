@{
    Layout = null;
}
@using TECOCITY_BGOI
@model  Tk_SupplierBas
<!DOCTYPE html>

<html>
<head>

    <meta name="renderer" content="webkit|ie-comp|ie-stand">
    <title>ReportPrint</title>
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery_dialog.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Table_Fix.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/cmm_PrintPageSet.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/SupplyManage/ReportPrint.js")" type="text/javascript"></script>
</head>
<body style="margin: 0 0 0 0; background-color: white;">
    <div id="pageContent" style="margin-left: 2px; margin-right: 2px; margin-top: 2px; overflow-y: auto;">
        <div id="tabTitile"><span style="margin-left: 10px;">流水号：@Model.Sid</span></div>@Html.HiddenFor(model => model.Sid)
        <div style="margin-top: 10px; font-size: 20px; font: 900; text-align: center; font-weight: bold">
            供应商处理记录表
        </div>
        <div style="height: auto; margin-left: 8px; width: 98.5%; margin-top: 10px;">
            <table id="myTable" class="tabInfo" style="text-align: center;">
                <tr style="text-align: center;">
                    @* <td style="width: 20px;">序号 </td>*@
                    <td style="width: 15%;">供应商类别</td>
                    <td style="width: 15%;">公司名称</td>
                    <td style="width: 15%;">公司简称</td>
                    <td style="width: 15%;">所属地区</td>
                    <td style="width: 15%;">所属国家</td>
                    <td style="width: 15%;">公司注册地址</td>
                    <td style="width: 15%;">公司创建时间</td>
                    <td style="width: 15%;">税务登记号</td>
                    <td style="width: 15%;">营业执照号码</td>
                    <td style="width: 15%;">公司办公地址 </td>
                </tr>
                <tbody id="DetailInfo" class="tabInfo"></tbody>
                <tr style="text-align: center;">
                    <td style="width: 15%;">法人代表 </td>
                    <td style="width: 15%;">公司出厂/出货地址 </td>
                    <td style="width: 15%;">组织机构代码 </td>
                    <td style="width: 15%;">注册资金 </td>
                    <td style="width: 15%;">开户行名称 </td>
                    <td style="width: 15%;">银行账号 </td>
                    <td style="width: 15%;">公司总人数</td>
                    <td style="width: 15%;">企业类型 </td>
                    <td style="width: 15%;">业务分布 </td>
                    <td style="width: 15%;">开票方式 </td>

                </tr>
                <tbody id="Detail" class="tabInfo"></tbody>
                <tr style="text-align: center;">
                    <td style="width: 15%;">去年营业额 </td>
                    <td style="width: 15%;">研发人数 </td>
                    <td style="width: 15%;">质检人数</td>
                    <td style="width: 15%;">生产人数 </td>
                    <td style="width: 15%;">供需关系 </td>
                    <td style="width: 15%;">工作开始时间 </td>
                    <td style="width: 15%;">工作结束时间 </td>
                    <td style="width: 15%;">是否有健全的组织机构 </td>
                    <td style="width: 15%;">工作日开始时间 </td>
                    <td style="width: 15%;">工作日结束时间 </td>

                </tr>
                <tbody id="Detail3" class="tabInfo" style="text-align: left;"></tbody>
                <tr style="text-align: center;">
                    <td style="width: 15%;">经营范围 </td>
                    <td style="width: 15%;">拟购产品行业排名 </td>
                    <td style="width: 15%;">排名 </td>
                    <td style="width: 15%;">名次 </td>
                    <td style="width: 15%;">供应商规模和经营品种分类 </td>
                    <td style="width: 15%;">产品质量执行标准 </td>
                    <td style="width: 15%;">是否有代理产品授权证明 </td>
                    <td style="width: 15%;">是否产品生产厂家图纸 </td>
                    <td style="width: 15%;">代理产品国内所属级别 </td>
                    <td style="width: 15%;">代理进口产品进货证明材料 </td>
                </tr>
                <tbody id="Detail4" class="tabInfo" style="text-align: left;"></tbody>
                <tbody id="Detail5" class="tabInfo" style="text-align: left;"></tbody>
            </table>

        </div>
        <div style="text-align: right; margin-top: 15px; width: 99%;">
            <input type="button" id="btnPrint" value="打 印" class="btn" />
            <br />
        </div>
    </div>
</body>
</html>
