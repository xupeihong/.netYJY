@{
    Layout = null;
}
@using TECOCITY_BGOI
<!DOCTYPE html>

<html>
<head>
   @* <meta name="viewport" content="width=device-width" />*@
    <meta name="renderer" content="webkit|ie-comp|ie-stand">
    <title>ApprovalWarn</title>
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery_dialog.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/ui.jqgrid.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.jqGrid.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/SupplyManage/ApprovalWarn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/My97DatePicker/WdatePicker.js")" type="text/javascript" charset=“UTF-8″></script>
    <style>
        .ui-jqgrid tr.jqgrow td {
            white-space: normal !important;
            height: auto;
            vertical-align: text-top;
            padding-top: 2px;
        }
    </style>
</head>
<body>
    @using (Html.BeginForm("ApprovalWarn", "SupplyManage", FormMethod.Post, new { id = "GForm" }))
    {
        <div id="pageContent">
            <div class="mainHead">
                <img src="../images/house.png" class="house" />&nbsp;&nbsp;&nbsp;&nbsp;当前位置>><span style="color: #000099;">证书管理</span>>>证书有效期提醒
            </div>
            <div id="search" style="height: 50px;">
                <table class="tabSearch" cellpadding="0" cellspacing="0">
                    <tr>
                        <td>用户姓名 @Html.TextBox("UserName") &nbsp;&nbsp;&nbsp;&nbsp;业务类型 @Html.DropDownList("BusinessType", EquipMan.GetNewConfigContent("BusinessType"), new { @class = "txtCss" }) &nbsp;&nbsp;&nbsp;&nbsp;专业技术资格名称 @Html.TextBox("TecoName", "", new { @class = "txtCss" })<input class="btn" type="button" value="查  询" onclick="reload()" /></td>
                    </tr>
                </table>
            </div>
            <input class="btnL" type="button" id="TimeOut" style="margin-top: 10px; margin-left: 10px;" value="设置提醒时间" />
            <div id="bor" style="width: 100%; margin-top: 5px; margin-left: 10px;">
                <table id="list" class="scroll" style="width: 100%;"></table>
                <div id="pager" class="scroll" style="text-align: center; width: 100%;"></div>
            </div>
        </div>
                    
    }
</body>
</html>
