        .ui-jqgrid tr.jqgrow td {
            white-space: normal !important;
            height: auto;
            vertical-align: text-top;
            padding-top: 2px;
        }

        .mainHead {
            position: absolute;
            float: left;
        }

        html, body {
            overflow: auto;
            margin: 0px;
            width: 100%;
            height: 100%;
        }
    </style>
