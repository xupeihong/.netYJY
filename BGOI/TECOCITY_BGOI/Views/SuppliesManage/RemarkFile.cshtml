@{
    Layout = null;
}
@using TECOCITY_BGOI
@model tk_SFileInfo
<!DOCTYPE html>

<html>
<head>
    @* <meta name="viewport" content="width=device-width" />*@
    <meta name="renderer" content="webkit|ie-comp|ie-stand">
    <meta http-equiv="Pragma" content="no-cache" />
    <meta http-equiv="Cache-Control" content="no-cache" />
    <meta http-equiv="Expires" content="0" />
    <title>RemarkFile</title>
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/SupplyManage/RemarkFile.js")" type="text/javascript"></script>
</head>
@using (Html.BeginForm("RemarkFile", "SuppliesManage", FormMethod.Post, new { id = "updateFile", action = "RemarkFileMsg", enctype = "multipart/form-data" }))
{
    <body>
        @Html.Hidden("msg", @ViewData["msg"])
        @Html.HiddenFor(model => model.Createuser)
        @Html.HiddenFor(model => model.Createtime)
        @Html.HiddenFor(model => model.Validate)
        @Html.Hidden("FID")
        <div id="hole" style="overflow-y: auto;">
            <div id="tabTitile"><span style="margin-left: 10px;">流水号：@Model.Sid</span></div>@Html.HiddenFor(model => model.Sid)
            <table class="tabInfo" style="width: 97%; margin-left: 8px; margin-top: 10px;">
                <tr>
                    <td class="textLeft">内容项</td>
                    <td class="textRight">@Html.DropDownListFor(model => model.Typeo, SupplyManage.GetConfigType("Typeo"), new { @class = "txtCss" }) </td>
                    <td class="textLeft">其他类说明</td>
                    <td class="textRight">@Html.DropDownListFor(model => model.Ftype, SupplyManage.GetConfigType("FType"), new { @class = "txtCss" }) </td>

                </tr>
                <tr>
                    <td class="textLeft">文件具体项目</td>
                    <td class="textRight">@Html.DropDownListFor(model => model.Item, SupplyManage.GetConfigType("Item"), new { @class = "txtCss" }) </td>
                    <td class="textLeft">其他项说明</td>
                    <td class="textRight">@Html.TextBoxFor(model => model.Itemo, new { @class = "txtCss", @disabled = "disabled" }) </td>
                </tr>
                <tr>
                    <td class="textLeft">上传资质文档</td>
                    <td class="textRight" colspan="3">

                        <input name="file" type="file" id="UploadFile" class="file" />
                        <div id="Progress" runat="server" style="float: right; width: 85%; color: Green; height: 24px; line-height: 24px;">
                        </div>
                    </td>
                </tr>
            </table>
            <div>
                <table>
                    <tr>
                        <td>
                            <input type="button" value="更新" id="UPFiles" class="btn" style="cursor: pointer; margin-left: 700px; margin-top: 10px;" />
                        </td>
                    </tr>
                </table>
            </div>
        </div>
    </body>
}
</html>
