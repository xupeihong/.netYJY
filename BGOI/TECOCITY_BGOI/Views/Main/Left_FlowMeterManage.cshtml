@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Left_FlowMeterManage</title>

    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-leftmenu.js")" type="text/javascript"></script>
</head>
<body>
    <dl id="wenzhang">
        <dd>
            <dl>
                <dt><span>▪ 流量计维修</span></dt>
                <dd>
                    <ul>
                        <li onclick="TurnTo('../FlowMeterManage/CardManage')"><span>- 登记卡管理</span></li>
                        <li onclick="TurnTo('../FlowMeterManage/WorkCardManage')"><span>- 随工单管理</span></li>
                        <li onclick="TurnTo('../FlowMeterManage/TransCardManage')"><span>- 流转卡管理</span></li>
                        <li><span onclick="TurnTo('../FlowMeterManage/DeliveryManage')">- 收货管理</span></li>
                        <li><span onclick="TurnTo('../FlowMeterManage/TaskManagement')">- 任务管理</span></li>
                        <li><span>+ 检测管理</span></li>
                        <dl id="son">
                            <dd>
                                <ul>
                                    <li onclick="TurnTo('../FlowMeterManage/CheckData?RepairType=进厂检测')"><span>▪ 进厂检测</span></li>
                                    <li onclick="TurnTo('../FlowMeterManage/CheckData?RepairType=清洗检测')"><span>▪ 清洗检测</span></li>
                                    <li onclick="TurnTo('../FlowMeterManage/CheckData?RepairType=维修检测')"><span>▪ 维修检测</span></li>

                                </ul>
                            </dd>
                        </dl>
                        <li onclick="TurnTo('../FlowMeterManage/CleanRepair')"><span>- 清洗管理</span></li>
                        <li onclick="TurnTo('../FlowMeterManage/RepairInfo')"><span>- 维修管理</span></li>
                        <li onclick="TurnTo('../FlowMeterManage/InspectionManage')"><span>- 送检管理</span></li>
                        <li><span>+ 报价管理</span></li>
                        <dl id="son">
                            <dd>
                                <ul>
                                    <li onclick="TurnTo('../FlowMeterManage/Quotation')"><span>▪ 报价单</span></li>
                                    <li onclick="TurnTo('../FlowMeterManage/Servicing')"><span>▪ 确认单</span></li>


                                </ul>
                            </dd>
                        </dl>
                        <li onclick="TurnTo('../FlowMeterManage/SendManage')"><span>- 发货管理</span></li>
                        <li onclick="TurnTo('../FlowMeterManage/PayMentManage')"><span>- 缴费管理</span></li>
                    </ul>
                </dd>
            </dl>
        </dd>
        <dd>
            <dl>
                <dt><span>▪ 汇总统计</span></dt>
                <dd>
                    <ul>
                        <li onclick="TurnTo('../FlowMeterManage/SearchSchedule')"><span>- 维修台账</span></li>
                        <li onclick="TurnTo('../FlowMeterManage/ScheduleDetail')"><span>- 维修进度情况</span></li>
                        <li onclick="TurnTo('../FlowMeterManage/SearchService')"><span>- 缴费明细汇总表</span></li>
                    </ul>
                </dd>
            </dl>
        </dd>
        <dd>
            <dl>
                <dt><span>▪ 数据分析</span></dt>
                <dd>
                    <ul>
                        <li><span>+ 检测分析</span></li>
                        <dl id="son">
                            <dd>
                                <ul>
                                    <li onclick="TurnTo('../FlowDAManage/DetectionCurve')"><span>▪ 检测对比曲线图</span></li>
                                    <li onclick="TurnTo('../FlowDAManage/RepeatValueReport')"><span>▪ 重复性和示值报告</span></li>
                                </ul>
                            </dd>
                        </dl>
                        <li><span>+ 故障点分析</span></li>
                        <dl id="son">
                            <dd>
                                <ul>
                                    <li onclick="TurnTo('../FlowDAManage/CustomerAnaly')"><span>▪ 大客户数据分析</span></li>
                                    <li onclick="TurnTo('../FlowDAManage/UserAnaly')"><span>▪ 问题数量分析</span></li>
                                    <li onclick="TurnTo('../FlowDAManage/YearAnaly')"><span>▪ 按年份分析</span></li>
                                    <li onclick="TurnTo('../FlowDAManage/CalibreAnaly')"><span>▪ 按口径分析</span></li>
                                </ul>
                            </dd>
                        </dl>
                    </ul>
                </dd>
            </dl>
        </dd>
        <dd>
            <dl>
                <dt><span>▪ 系统设置</span></dt>
                <dd>
                    <ul>
                        <li onclick="TurnTo('../FlowSystem/BasicSet')"><span>- 公共信息配置</span></li>
                        <li onclick="TurnTo('../FlowSystem/GroupSet')"><span>- 下发小组人员配置</span></li>
                    </ul>
                </dd>
            </dl>
        </dd>

    </dl>
</body>
</html>


