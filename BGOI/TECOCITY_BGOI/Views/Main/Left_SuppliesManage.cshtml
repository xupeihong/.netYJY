@using TECOCITY_BGOI;
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Left_SuppliesManage</title>
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-leftmenu1.js")" type="text/javascript"></script>
</head>
<body>
    @{
        string exjob = GAccount.GetAccountInfo().Exjob;
        int userid = GAccount.GetAccountInfo().UserID;
        string Rights = GAccount.GetAccountInfo().Rights;
        string username = GAccount.GetAccountInfo().UserName;
    }
    <dl id="wenzhang">
        <dd>

            <dl>
                <dt><span>▪ 供应商管理</span></dt>
                <dd>
                    <ul>
                        <li onclick="TurnTo('../SuppliesManage/SuppliesPrepare');Back(this)"><span>- 新增供应商</span></li>
                        <li onclick="TurnTo('../SuppliesManage/SupplyMan');Back(this)" id="WaitSupply" style="background-color: #ddd6d6;"><span>- 部门内部评审</span></li>
                        @* @if (exjob != "")
                        { 
                       *@
                        <li onclick="TurnTo('../SuppliesManage/Certificate');Back(this)" id="Certify"><span>- 公司准入评审</span></li>
                        @* }*@
                        <li onclick="TurnTo('../SuppliesManage/SupplyOK');Back(this)" id="SupplyOK"><span>- 合格供应商管理</span></li>
                        <li onclick="TurnTo('../SuppliesManage/SupplyWeigui');Back(this)" id="SupplyWeigui"><span>- 已淘汰供应商管理</span></li>
                        @*@if (exjob != "")
                        {
                            *@<li onclick="TurnTo('../SuppliesManage/SupplyApproval');Back(this)" id="SupplyApproval"><span>- 供应商年度评价管理</span></li>
                        @* }*@
                    </ul>
                </dd>
            </dl>

            <dl>
                <dt><span>▪ 客户管理</span></dt>
                <dd>
                    <ul>
                        <li onclick="TurnTo('../SuppliesManage/CustemerMang');Back(this)" id="Custemer"><span>- 客户管理</span></li>
                    </ul>
                </dd>
            </dl>
            <dl>
                <dt><span>▪ 合同管理</span></dt>
                <dd>
                    <ul>
                        <li onclick="TurnTo('../Contract/ContractMaintain');Back(this)"><span>- 合同维护</span></li>
                        <li onclick="TurnTo('../Contract/StayReturnCash');Back(this)"><span>- 待结算提醒</span></li>
                        <li onclick="TurnTo('../Contract/ContractStandingBook');Back(this)"><span>- 合同台账</span></li>
                    </ul>
                </dd>
            </dl>
            <dl>
                <dt><span>▪ 仪器设备管理</span></dt>
                <dd>
                    <ul>
                        <li onclick="TurnTo('../EquipMan/EquipMaintain');Back(this)"><span>- 仪器设备维护</span></li>
                        <li onclick="TurnTo('../EquipMan/EquipWarn');Back(this)"><span>- 仪器设备提醒</span></li>
                        <li onclick="TurnTo('../EquipMan/StandingBook');Back(this)"><span>- 台账</span></li>

                    </ul>
                </dd>
            </dl>
            <dl>
                <dt><span>▪ 持证资质管理</span></dt>
                <dd>
                    <ul>
                        <li onclick="TurnTo('../UserAptitude/AptitudeMaintain');Back(this)"><span>- 维护</span></li>
                        <li onclick="TurnTo('../UserAptitude/AptitudeWarn');Back(this)"><span>- 持证有效期提醒</span></li>
                    </ul>
                </dd>
            </dl>
            <dl>
                <dt><span>▪ 系统设置</span></dt>
                <dd>
                    <ul>
                        @if (userid == 132)
                        {
                            <li onclick="TurnTo('../System/AppSet?id=BGOI_BasMan/tk_AppContent');Back(this)"><span>- 审批设置</span></li>
                        }
                        <li onclick="TurnTo('../SystemManage/BasInformation');Back(this)"><span>- 基本信息设置</span></li>
                    </ul>
                </dd>
            </dl>
            @if (username == "teco")
            {
                <dl>
                    <dt><span>▪ 非合格供应商</span></dt>
                    <dd>
                        <ul>

                            <li onclick="TurnTo('../SuppliesManage/IsnotSuppliesPrepare');Back(this)"><span>-非合格供应商</span></li>

                        </ul>
                    </dd>
                </dl>
            }
        </dd>
    </dl>
</body>
</html>
