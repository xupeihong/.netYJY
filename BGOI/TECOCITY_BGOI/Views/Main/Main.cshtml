@{
    Layout = null;
}
@using TECOCITY_BGOI
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Main</title>
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-leftmenu.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-leftmenu1.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Main/Main.js")" type="text/javascript"></script>
</head>
<body>
    @{
        string unitId = GAccount.GetAccountInfo().UnitID;
        
    }
    @Html.Hidden("Role", @ViewData["Role"])
    @Html.Hidden("UnitId", unitId)


    @Html.Hidden("UserID", @ViewData["UserId"])
    @Html.Hidden("UserName", @ViewData["UserName"])
    @Html.Hidden("EJob", @ViewData["ExJob"])
    <div id="pageAll">
        <div id="Header">
            <div class="Header_l" style="margin-top: 2px;">
                <img src="../images/logo.png" class="img" />
            </div>
            <div class="Header_m">
                <img src="../images/title1.png" class="img2" />
            </div>
            <div class="Header_r">
                <img src="../images/title2.png" class="img3" />
            </div>
            <div id="rightMake" style="float: right;  height: 40px; line-height: 40px; margin-top: 20px; color: white; font-weight: bold;">
                <img src="../images/fanhui.png" style="margin-left: 20px;" /><a style="color: white; text-decoration: none; cursor: pointer; margin-left: 2px;" onclick="window.open('../Main/SelectModuleYS')">返回主页</a><img src="../images/tuichu.png" style="margin-left: 20px;" /><a style="color: white; text-decoration: none; margin-left: 2px;" href='../Account/Login'>注销</a><img src="../images/zhuxiao.png" style="margin-left: 20px;" /><a style="cursor: pointer; margin-left: 2px;" onclick="window.close() ">退出</a><img src="../images/xiugai.png" style="margin-left: 20px;" /><a style="margin-left: 2px; cursor: pointer;"  onclick="UpdatePwd() ">修改密码</a><img src="../images/chongzhi.png" id="ReImage" style="margin-left: 20px;display:none;" /><a style="margin-left: 2px;display:none; cursor: pointer;" id="ReA" onclick="RestPwd() ">重置密码</a>
            </div>
        </div>
        <div id="FMenu">
            <div id="M1"></div>
            <div id="M2">
                <div class="lman">
                    <img src="../images/littleman.png" />
                </div>
                <div id="logman">当前登录:@ViewData["UserName"] </div>
                <div id="logunit">@ViewData["Unit"]</div>
                <div id="logname">@ViewData["ExJob"]</div>
                <div id="localtime"></div>
            </div>
            <div id="M3"></div>
            <div id="M4"></div>
        </div>
        <div id="MFun">
            <div id="FunL">
                <iframe id="iframeLeftTree" name="iframeLeftTree" src="" frameborder="no" allowtransparency="true" style="width: 100%; height: 100%; background: transparent;" scrolling="auto"></iframe>

            </div>
            <div id="FunR">
                <iframe id="iframeRight" name="iframeRight" frameborder="no" allowtransparency="true" style="width: 100%; height: 100%; background: transparent;" scrolling="auto"></iframe>
            </div>
        </div>
        <div id="footer">
        </div>
        <div id="alarm" style="display: none; position: absolute; right: 10px; bottom: 5px; height: 100px; width: 220px; background-color: white; border: 2px solid #3598db;">
            <div style="width: 100%; height: 20px; background-color: #3598db; line-height: 20px;"><a onclick="closeAlarm()" style="color: white; font-weight: bold; padding-left: 180px; cursor: pointer;">关 闭</a></div>
            <div id="content" style="margin-left: 15px; color: #3598db; font-weight: bold; overflow-y: auto;"></div>
        </div>
        <div id="divMessage" style="float: right; display: none; width: 220px; background-color: white; height: 100px; position: absolute; margin-right: 15px;">
            <div style="width: 100%; height: 20px; background-color: #3598db; line-height: 20px;"><a onclick="CloseMessage()" style="color: white; font-weight: bold; padding-left: 180px; cursor: pointer;">关 闭</a></div>
            <div id="noticecon" style="margin-top: 10px; text-align: center;">
            </div>
        </div>


        <div id="divReceivePaymentMessage" style="float: right; display: none; width: 220px; background-color: white; height: 100px; position: absolute; margin-right: 15px;">
            <div style="width: 100%; height: 20px; background-color: #3598db; line-height: 20px;"><a onclick="CloseReceivePaymentMessage()" style="color: white; font-weight: bold; padding-left: 180px; cursor: pointer;">关 闭</a></div>
            <div id="ReceivePayment" style="height: 100px; margin-top: 10px; text-align: center; overflow-y: auto">
            </div>
        </div>
        <div id="divRetailMessage" style="float: right; display: none; width: 220px; background-color: white; height: 100px; position: absolute; margin-right: 15px;">
            <div style="width: 100%; height: 20px; background-color: #3598db; line-height: 20px;"><a onclick="CloseRetailMessage()" style="color: white; font-weight: bold; padding-left: 180px; cursor: pointer;">关 闭</a></div>
            <div id="noticecon2" style="height: 100px; margin-top: 10px; text-align: center; overflow-y: auto">
            </div>
        </div>


        <div id="SupAlarm" style="float: right; overflow-y: auto; display: none; width: 220px; background-color: white; height: 100px; position: absolute; margin-right: 240px; margin-bottom: 5px; cursor: pointer;">
            <div style="width: auto; height: 20px; background-color: #3598db; line-height: 20px;"><a onclick="ClosePage()" style="color: white; font-weight: bold; padding-left: 170px; cursor: pointer; width: auto;">关 闭</a></div>
            <div id="NoticeContent" style="margin-top: 10px; text-align: center; cursor: pointer; overflow-y: auto; height: auto;">
            </div>
        </div>
        <div id="UpdateBas" style="float: right; overflow-y: auto; display: none; width: 220px; background-color: white; height: 100px; position: absolute; margin-right: 470px; margin-bottom: 5px; cursor: pointer;">
            <div style="width: auto; height: 20px; background-color: #3598db; line-height: 20px;"><a onclick="ClosePages()" style="color: white; font-weight: bold; padding-left: 170px; cursor: pointer; width: auto;">关 闭</a></div>
            <div id="BasContent" style="margin-top: 10px; text-align: center; cursor: pointer; overflow-y: auto;">
            </div>
        </div>
         <div id="ConHTSP" style="display: none; position: absolute; right: 10px; bottom: 5px; height:auto; width: 220px; background-color: white; border: 2px solid #3598db;">
            <div style="width: 100%; height: 20px; background-color: #3598db; line-height: 20px;"><a onclick="closeCon()" style="color: white; font-weight: bold; padding-left: 180px; cursor: pointer;">关 闭</a></div>
            <div id="ConContent" style="margin-left: 15px; color: #3598db; font-weight: bold; overflow-y: auto;"></div>
        </div>
           <div id="alarm1" style="display: none; position: absolute; right: 10px; bottom: 5px; height: auto; width: 220px; z-index: 999; background-color: white; border: 2px solid #3598db;">
                <div style="width: 100%; height: 20px; background-color: #3598db; line-height: 20px;"><a onclick="closeKC()" style="color: white; font-weight: bold; padding-left: 180px; cursor: pointer;">关 闭</a></div>
                <div id="content1" style="margin-left: 15px; color: #3598db; font-weight: bold; overflow-y: auto; height: auto"></div>
            </div>
    </div>
</body>
</html>


