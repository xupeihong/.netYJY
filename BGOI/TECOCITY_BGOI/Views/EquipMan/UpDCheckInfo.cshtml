@{
    Layout = null;
}
@{
    ViewBag.Title = "";
}
@using TECOCITY_BGOI;
@model DCheckInfo
<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Pragma" content="no-cache" />
    <meta http-equiv="Cache-Control" content="no-cache" />
    <meta http-equiv="Expires" content="0" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/EquipMan/UpDCheckInfo.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/My97DatePicker/WdatePicker.js")" type="text/javascript" charset=“UTF-8″></script>

</head>
@using (Html.BeginForm("UpDCheckInfo", "EquipMan", FormMethod.Post, new { id = "ProjectformInfo" }))
{
    <body>@Html.Hidden("Did", @ViewData["Did"])  @Html.Hidden("num", @ViewData["num"])
        <div id="hole" style="overflow-y: auto;">
            @Html.HiddenFor(model => model.StrECode)
            <table class="tabInfo" style="width: 97%; margin-left: 8px; margin-top: 10px;">
                <tr>
                    <td class="textLeft">检定日期</td>
                    <td class="textRight">@Html.TextBoxFor(model => model.StrCheckDate, new { @class = "txtCss", onclick = "WdatePicker({dateFmt: 'yyyy-MM-dd' })" })<span style="color: red">*</span></td>
                </tr>
                <tr>
                    <td class="textLeft">检定方式</td>
                    <td class="textRight">@Html.DropDownListFor(model => model.StrCheckWay, EquipMan.GetNewConfigContent("CheckWay"), new { @class = "txtCss" })<span style="color: red">*</span></td>
                </tr>
                <tr>
                    <td class="textLeft">检定/校准单位</td>
                    <td class="textRight">@Html.TextBoxFor(model => model.StrCheckCompany, new { @class = "txtCss" })<span style="color: red">*</span></td>
                </tr>
                <tr>
                    <td class="textLeft">费用</td>
                    <td class="textRight">@Html.TextBoxFor(model => model.StrCharge, new { @class = "txtCss" })<span style="color: red">*</span></td>
                </tr>
                <tr>
                    <td class="textLeft">精度</td>
                    <td class="textRight">@Html.TextBoxFor(model => model.StrPrecision, new { @class = "txtCss" })</td>
                </tr>
                <tr>
                    <td class="textLeft">负责人</td>
                    <td class="textRight">@Html.TextBoxFor(model => model.StrPrincipal, new { @class = "txtCss" })</td>
                </tr>
                <tr>
                    <td class="textLeft">备注</td>
                    <td class="textRight">@Html.TextAreaFor(model => model.StrCensorshipRemark, new { style = "width:90%;height:100px;" })</td>
                </tr>
                <tr>
                    <td class="textLeft">结果</td>
                    <td class="textRight">@Html.TextAreaFor(model => model.StrCalibrationResults, new { style = "width:90%;height:100px;" })</td>
                </tr>
            </table>


            <div style="margin-top: 10px; margin-left: 80%">
                <input class="btn" type="button" id="charge" value="确定" />
            </div>

        </div>
    </body>
       
}
</html>
