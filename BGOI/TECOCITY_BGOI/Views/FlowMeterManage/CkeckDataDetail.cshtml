@{
    Layout = null;
}
@using TECOCITY_BGOI;
@model tk_CheckData
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>CkeckDataDetail</title>


    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery_dialog.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/ui.jqgrid.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Validate/Validate-Injection.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.jqGrid.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/My97DatePicker/WdatePicker.js")" type="text/javascript"></script>
    <style>
        .textRightL {
            width: 150px;
        }
    </style>
    <script>
        function a() {
            var r = document.getElementById('HighorLow').innerText.split('-')[1];
            
            if (r == "高频")
                document.getElementById('HighorLow2').innerHTML = "平均仪表系数1/m³ %";
            else
                document.getElementById('HighorLow2').innerHTML = "示值误差 %";
        }
    </script>
</head>
<body onload="a()">
    @using (Html.BeginForm("CkeckDataDetail", "FlowMeterManage", FormMethod.Post, new { id = "ProjectformInfo", onsubmit = "return returnConfirm();" }))
    {


        <div id="pageContent" style="overflow-y:auto;">
            <div style="width:98%;margin-left:10px;margin-top:5px;">
             
                <table class="tabInfo" id="content">
                    <tr style="height:30px;">
                        <td colspan="6" style="text-align:center; font-size:20px; font-weight:bold;">@Html.LabelFor(model => model.StrRepairType, @Model.StrRepairType, new { @class = "txtCss" })</td>
                    </tr>
                    @Html.Hidden("RepairType")
                  

                    <tr style="height:30px;">




                        @Html.HiddenFor(model => model.StrRID)

                        <td class="textLeftL">检测人</td>
                        <td class="textRightL">
                            @Model.StrCheckUser

                        </td>

                        <td class="textLeftL">检测时间</td>
                        <td class="textRightL">
                            @Model.StrCheckDate


                        </td>
                        <td class="textLeftL">检测方式</td>
                        <td class="textRightL">
                            <lable id="HighorLow">@Model.StrRepairMethod</lable>

                        </td>
                    </tr>
                  



                    <tr style="height:30px;">
                        <td class="textLeftL">qt<=q<=qMax示值误差(%)</td>
                        <td class="textRightL">
                            @Model.Strq1
                        </td>
                        <td class="textLeftL">qMin<=q<=qt示值误差(%) </td>
                        <td class="textRightL">
                            @Model.Strq2
                        </td>
                        <td class="textLeftL">重复性平均值</td>
                        <td class="textRightL">
                            @Model.StrAvgRepeat
                        </td>

                    </tr>

                    <tr style="height:30px;">
                        <td class="textLeftL">拍照示值误差(%)</td>
                        <td class="textRightL">
                            @Model.StrPDeviation
                        </td>
                        <td class="textLeftL">原仪表系数</td>
                        <td class="textRightL">
                            @Model.StrOratio
                        </td>
                        <td class="textLeftL">数据是否合格</td>
                        <td >
                            @Model.Strvalidate
                        </td>
                    </tr>
                    <tr>

                        <td class="textLeftL">测试结果备注</td>
                        <td class="textRightL" colspan="5">
                            @Model.StrRemark
                        </td>
                       
                    </tr>
                    <tr style="height:30px;text-align:center">
                        <td class="textLeftL" colspan="2">流量点m³/h</td>
                        <td class="textLeftL" colspan="2"><lable id="HighorLow2">平均仪表系数1/m³ %</lable></td>
                        <td class="textLeftL" colspan="2">重复性 %</td>

                    </tr>
                    <tr style="height:30px;text-align:center">

                        <td class="textRightL" colspan="2">
                            @Model.StrQmin
                        </td>

                        <td class="textRightL" colspan="2">
                            @Model.StrAvg_Qmin
                        </td>

                        <td class="textRightL" colspan="2">
                            @Model.StrRepeat_Qmin
                        </td>

                    </tr>
                    <tr style="height:30px;text-align:center">

                        <td class="textRightL" colspan="2">
                            @Model.StrQmax1
                        </td>

                        <td class="textRightL" colspan="2">
                            @Model.StrAvg_Qmax1
                        </td>

                        <td class="textRightL" colspan="2">
                            @Model.StrRepeat_Qmax1
                        </td>

                    </tr>
                    <tr style="height:30px;text-align:center">

                        <td class="textRightL" colspan="2">
                            @Model.StrQmax2
                        </td>

                        <td class="textRightL" colspan="2">
                            @Model.StrAvg_Qmax2
                        </td>

                        <td class="textRightL" colspan="2">
                            @Model.StrRepeat_Qmax2
                        </td>

                    </tr>
                    <tr style="height:30px;text-align:center">

                        <td class="textRightL" colspan="2">
                            @Model.StrQmax25
                        </td>

                        <td class="textRightL" colspan="2">
                            @Model.StrAvg_Qmax25
                        </td>

                        <td class="textRightL" colspan="2">
                            @Model.StrRepeat_Qmax25
                        </td>

                    </tr>
                </table>
            </div>

        </div>

    }
</body>
</html>
