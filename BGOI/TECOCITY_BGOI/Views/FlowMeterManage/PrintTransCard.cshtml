@{
    Layout = null;
}
@using TECOCITY_BGOI;
@model RepairCard

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>PrintTransCard</title>

    <link href="@Url.Content("~/CSS/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/Print.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery_dialog.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/ui.jqgrid.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")"  type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.jqGrid.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/cmm_PrintPageSet.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/FlowMeterManage/PrintTransCard.js")" type="text/javascript"></script>
</head>
<body style="margin:0 0 0 0; background-color:white; overflow:auto;" >
    <div id="pageContent" style=" margin-left:2px; margin-right:2px; margin-top:2px; ">

        <div id="ReportContent" style="margin-top:10px; page-break-after: always;">
            <div style="width:100%; text-align:center; font-size:18px;">北京市燕山工业燃气设备有限公司</div>
            <div style="font-size:22px; font-weight:bold; float:left; margin-left:37%;">流量计维修流转卡</div>
            <div style="width:100%; margin-left:10px; float:left;">
                <div style="font-size:16px; font-weight:bold; float:left;width:70%; margin-left:10px;">客户名称：@Model.strCustomerName</div>
                <div style="width:28%; float:left; text-align:right;">维修编号：@Model.strRepairID</div>
            </div>
            <div style="font-size:16px; font-weight:bold; float:left; margin-left:20px;">安装地点：@Model.strNewUnit</div>
            <table id="search" class="tabInfoWork2">
                @Html.HiddenFor(model=>model.strRID)
                <tr style="height:30px;">
                    <td>仪表名称</td>
                    <td>@Model.strMeterName</td>
                    <td>生产厂家</td>
                    <td colspan="3">@Model.strManufacturer</td>
                    <td>精度等级</td>
                    <td>@Model.strPrecision</td>
                </tr>

                <tr style="height:30px;">
                    <td>仪表型号</td>
                    <td>@ViewData["Model"]</td>
                    <td>仪表编号</td>
                    <td colspan="3">@Model.strMeterID</td>
                    <td>出厂日期</td>
                    <td>@Model.strFactoryDate</td>
                </tr>

                <tr style="height:30px;">
                    <td>字轮读数</td>
                    <td>@Model.strRecordNum</td>
                    <td>流量范围</td>
                    <td>@Model.strFlowRange</td>
                    <td>承压等级</td>
                    <td>@Model.strPressure</td>
                    <td>口径</td>
                    <td>@Model.strCaliber</td>
                </tr>
                
                <tr style="height:30px;">
                    <td>修正仪型号</td>
                    <td>@ViewData["X_Model"]</td>
                    <td>修正仪编号</td>
                    <td colspan="3">@Model.strX_ID</td>
                    <td>生产厂家</td>
                    <td>@Model.strX_Manufacturer</td>
                </tr>
                
                <tr style="height:30px;">
                    <td>标况累计</td>
                    <td>@Model.strX_Standard</td>
                    <td>工况累计</td>
                    <td colspan="3">@Model.strX_Operating</td>
                    <td>生产日期</td>
                    <td>@Model.strX_FactoryDate</td>
                </tr>
                
                <tr style="height:30px;">
                    <td>压力</td>
                    <td>@Model.strX_Pressure</td>
                    <td>温度</td>
                    <td colspan="3">@Model.strX_Temperature</td>
                    <td>其它数据</td>
                    <td>@Model.strX_Data</td>
                </tr>

                <tr style="height:30px;">
                    <td>送修初检测</td>
                    <td>@ViewData["FirstCheck"]</td>
                    <td>送修</td>
                    <td>@ViewData["SendRepair"]</td>
                    <td>修后一次检测</td>
                    <td>@ViewData["LastCheck"]</td>
                    <td>一次返修</td>
                    <td>@ViewData["OneRepair"]</td>
                </tr>
                
                <tr style="height:30px;">
                    <td>修后二次检测</td>
                    <td>@ViewData["TwoCheck"]</td>
                    <td>二次送修</td>
                    <td>@ViewData["TwoRepair"]</td>
                    <td>修后三次检测</td>
                    <td>@ViewData["ThreeCheck"]</td>
                    <td>三次返修</td>
                    <td>@ViewData["ThreeRepair"]</td>
                </tr>

                <tr style="height:60px;">
                    <td>备注</td>
                    <td colspan="7" style="text-align:left;">@ViewData["Comments"]</td>
                </tr>
                
            </table>
            <br />
        </div>        
        <div style="text-align:right; margin-top:5px; width:99%;">
            <input type="button" id="btnPrint" value="打 印" class="btn" />
        </div>

    </div>
</body>
</html>
