@{
    Layout = null;
}
@using TECOCITY_BGOI;
@model tk_Payment

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>DelPayment</title>
    
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery_dialog.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/ui.jqgrid.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")"  type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.jqGrid.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/FlowMeterManage/DelPayment.js")" type="text/javascript"></script>
   <style>
       .ui-jqgrid tr.jqgrow td {
           white-space: normal !important;
           height: auto;
           vertical-align: text-top;
           padding-top: 2px;
       }
   </style>
</head>
 <body>
     <div id="pageContent">
         <div id="tabTitile"><span style="margin-left: 10px; float: left;">缴费单号：@Model.StrPayID</span></div>
         @Html.Hidden("Hidden",ViewData["QIDs"])
         @Html.HiddenFor(model=>model.StrPayID)
        <div style="width:98%;margin-left:7px;margin-top:5px;">
            <table class="tabInfo" id="content1" style="width:98%;" align="center" >
                <tr style="height:20px;"> 
                    <td class="textRightL" style="text-align:left;" colspan="4"> * 基本信息 </td>
                </tr>
                <tr style="height:30px;">
                    <td class="textLeftL">缴费单位</td>
                    <td class="textRightL">@Model.StrPayUnit</td> 
                    <td class="textLeftL">缴费人</td>
                    <td class="textRightL">@Model.StrPayPerson</td>
                </tr>
                <tr style="height:30px;">
                    <td class="textLeftL">缴费金额</td>
                    <td class="textRightL">@Model.StrPayMount</td>
                    <td class="textLeftL">缴费时间</td>
                    <td class="textRightL">@Model.StrPayDate</td> 
                </tr>
                <tr style="height:70px;"> 
                    <td class="textLeftL">备注</td>
                    <td class="textRightL" colspan="3">@Model.StrComments</td> 
                </tr>
                <tr style="height:20px;"> 
                    <td class="textRightL" colspan="4">
                        <span style="text-align:left;"> * 缴费报价单 </span>
                    </td>
                </tr>
                <tr>
                    <td class="textRightL" colspan="4">
                        <div id="bor" style="width:100%;margin-top:5px;">
                            <table id="list" class="scroll" style="width:98%;" align="center"></table>
                            <div id="pager" class="scroll" style="text-align:center;width:98%;"></div>
                        </div><br /> 
                    </td>
                </tr>
            </table>
        </div>
        <div style="margin-top:5px;margin-right:20px; float:right;">
            <input class="btn" type="button" id="QDCX" value="确定撤销"/> 
        </div>
            
    </div>
</body>
</html>
