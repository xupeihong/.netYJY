@{
    Layout = null;
}

<!DOCTYPE html>
@using TECOCITY_BGOI;
@model RepairCard
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ServicingDetail</title>

    <link href="@Url.Content("~/CSS/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/Print.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery_dialog.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/ui.jqgrid.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.jqGrid.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/cmm_PrintPageSet.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/FlowMeterManage/ServicingDetail.js")" type="text/javascript"></script>

</head>




<body style="margin:0 0 0 0; background-color:white;overflow:auto">
    <div id="pageContent" style=" margin-left:2px; margin-right:2px; margin-top:2px;overflow-y:scroll; " >

        <div id="ReportContent" style="margin-top:10px; page-break-after: always;">

            <div style="width: 100%; text-align: center; font-size: 15px;">流量计检修服务确认单（涡轮、腰轮）</div>

            <div style="margin-left: 73%; float: left;">编号：@Model.strRepairID</div>
            <table class="tabInfo2">
                @Html.Hidden("RID", @Model.strRID)
                @Html.Hidden("BakType")     @Html.Hidden("BakUnitPrice")

                @Html.Hidden("DeviceName")     @Html.Hidden("DeviceType")
                @Html.Hidden("Num")            @Html.Hidden("UnitPrice")
                @Html.Hidden("TotalPrice")     @Html.Hidden("Comments")
                <tr>
                    <td rowspan="5">用户资料</td>
                </tr>
                <tr>
                    <td>用户单位</td>
                    <td colspan="3">

                        @Model.strCustomerName
                    </td>
                    <td>负责人</td>
                    <td colspan="3">
                        @Model.strS_Name
                    </td>


                </tr>
                <tr>
                    <td>联系电话</td>
                    <td colspan="3">
                        @Model.strS_Tel
                    </td>
                    <td>检测时间</td>
                    <td colspan="3">
                        @Model.strS_Date
                    </td>
                </tr>
                <tr style="height: 30px;">
                    <td>用户单位地址</td>
                    <td colspan="7">
                        @Model.strCustomerAddr
                    </td>


                </tr>
                <tr>
                    <td>仪表（设备）安装详细地址：</td>
                    <td colspan="7">
                        @Model.strPreUnit

                    </td>
                </tr>
                <tr>
                    <td rowspan="8">
                        仪表<br />
                        设备<br />
                        档案
                    </td>
                </tr>
                <tr>
                    <td>基表类型</td>
                    <td>
                        @Model.strModel


                    </td>
                    <td>生产厂家</td>
                    <td>
                        @Model.strManufacturer
                    </td>
                    <td>基表型号</td>
                    <td>
                        @Model.strModel
                    </td>
                    <td>基表编号</td>
                    <td>
                        @Model.strMeterID
                    </td>
                </tr>
                <tr></tr>
                <tr>
                    <td>口径DN</td>
                    <td>
                        @Model.strCaliber
                    </td>
                    <td>出厂日期</td>
                    <td>
                        @Model.strFactoryDate
                    </td>
                    <td>流量范围</td>
                    <td colspan="3">
                        @Model.strFlowRange
                    </td>
                </tr>
                <tr>


                    <td>承压等级</td>
                    <td>
                        @Model.strPressure
                    </td>
                    <td>字轮读数</td>
                    <td colspan="2">
                        @Model.strRecordNum
                    </td>
                    <td>精度等级</td>
                    <td colspan="2">
                        @Html.LabelFor(model => model.strPrecision, @Model.strPrecision)
                    </td>
                </tr>
                <tr>
                    <td>修正仪编号</td>
                    <td>@Model.strX_ID</td>
                    <td>修正仪型号</td>
                    <td style="text-align: left;">@ViewData["X_Model"]</td>
                    <td>生产厂家</td>
                    <td style="text-align: left;">@Model.strX_Manufacturer</td>
                    <td>生产日期</td>
                    <td style="text-align: left;">@Model.strX_FactoryDate</td>
                </tr>
                <tr>
                    <td>标况累计</td>
                    <td>@Model.strX_Standard</td>
                    <td>工况累计</td>
                    <td style="text-align: left;">@Model.strX_Operating</td>
                    <td>压力</td>
                    <td>@Model.strX_Pressure</td>
                    <td>温度</td>
                    <td style="text-align: left;">@Model.strX_Temperature</td>
                </tr>

                <tr>

                    <td>其他</td>
                    <td colspan="7" style="text-align: left;">@Model.strX_Data</td>
                </tr>


                <tr>
                    <td>仪表状况和故障描述</td>
                    <td colspan="8"></td>
                </tr>

                <tr>
                    <td rowspan="6">分项价格</td>
                </tr>
                <tr>
                    <td colspan="2">
                        项目
                    </td>
                    <td colspan="2">价格（元）</td>
                    <td colspan="2">项目</td>
                    <td colspan="2">价格（元）</td>
                </tr>
                <tr style="height: 30px;">
                    <td colspan="2">清洗保养</td>
                    <td colspan="2">
                        <label id="qx"></label>
                    </td>
                    <td colspan="2">出厂标定</td>
                    <td colspan="2">
                        <label id="ccbd"></label>
                    </td>
                </tr>

                <tr style="height: 30px;">
                    <td colspan="2">维修</td>
                    <td colspan="2">
                        <label id="wx"></label>
                    </td>
                    <td colspan="2">代理标定送检</td>
                    <td colspan="2">
                        <label id="dlbd"></label>
                    </td>
                </tr>


                <tr style="height: 30px;">
                    <td colspan="2">气密性测试（PN10）</td>
                    <td colspan="2">
                        <label id="jc"></label>
                    </td>
                    <td colspan="2">实流标定保过（确保通过通州、采育2座高压实流标定站的标定）</td>
                    <td colspan="2">
                        <label id="slbd"></label>
                    </td>
                </tr>
                <tr>
                    <td>其他</td>
                    <td colspan="7">
                        法兰盘表面处理（进、出口）：<label id="qt"></label>
                    </td>
                </tr>

                <tbody id="taskList" class="tabInfoWork2">
                    <tr>
                        <td id="firsttd" rowspan="2">
                            维修更换<br />
                            零件明细<br />
                        </td>
                    </tr>
                    <tr>
                        <td>零部件名称</td>
                        <td>
                            型号规格
                        </td>
                        <td>
                            数量
                        </td>
                        <td>单价</td>

                        <td>价格（元）</td>
                        <td colspan="3">
                            备注
                        </td>
                    </tr>


                </tbody>
                <tfoot>

                    <tr>
                        <td colspan="2">费用总计（含增值税）</td>
                        <td colspan="7">
                            <label id="Total"></label>￥
                        </td>
                    </tr>
                </tfoot>
            </table>
            <div style="padding-left: 25%">
                <table style="text-align: left">
                    <tr>
                        <td>委托方：</td>

                        <td style="padding-left: 200px">承接方：北京公用事业科技发展公司</td>

                    </tr>
                    <tr>
                        <td>负责人签章：</td>

                        <td style="padding-left: 200px">负责人签章：</td>

                    </tr>
                    <tr>
                        <td>开户行：</td>

                        <td style="padding-left: 200px">开户行：农行安外大街支行</td>

                    </tr>

                    <tr>
                        <td>账号：</td>

                        <td style="padding-left: 200px">账号：041001040003716</td>

                    </tr>
                    <tr>
                        <td>税号：</td>

                        <td style="padding-left: 200px">税号：110105101165939</td>

                    </tr>
                    <tr>
                        <td>邮编：</td>

                        <td style="padding-left: 200px">邮编：100011</td>

                    </tr>
                    <tr>
                        <td>电话：</td>

                        <td style="padding-left: 200px">电话：64258578</td>

                    </tr>
                    <tr>
                        <td>地址：</td>

                        <td style="padding-left: 200px">地址  ：北京市朝阳区安外外馆东后街35号</td>

                    </tr>

                </table>


            </div>
        </div>



        <div style="text-align: right; margin-top: 5px; width: 99%;">

            <input type="button" id="btnPrint" value="打 印" class="btn" />
        </div>
    </div>
</body>
</html>
