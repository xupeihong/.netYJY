@{
    Layout = null;
}
@using TECOCITY_BGOI;
@model tk_CleanRepair
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>CleanRepairDetail</title>
    <link href="@Url.Content("~/CSS/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery_dialog.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/ui.jqgrid.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Validate/Validate-Injection.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.jqGrid.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/My97DatePicker/WdatePicker.js")" type="text/javascript" charset="utf-8"></script>
    <script src="@Url.Content("~/Scripts/FlowMeterManage/CleanRepairDetail.js")" type="text/javascript"></script>
</head>
<body>
    @using (Html.BeginForm("CleanRepairDetail", "FlowMeterManage", FormMethod.Post, new { id = "ProjectformInfo", onsubmit = "return returnConfirm();" }))
    {


        <div id="pageContent" style="overflow-y:auto;">


            <div style="width:98%;margin-left:10px;margin-top:5px;">
                <table class="tabInfo" id="content">
                    <tr style="height:30px;">
                        <td colspan="6" style="text-align:center; font-size:20px; font-weight:bold;">清洗记录</td>
                    </tr>
                  
                    <tr style="height:30px;">
                        @Html.HiddenFor(model => model.StrCleanID, new { @class = "txtCss" })
                        <td class="textLeftL">登记卡号</td>
                        <td class="textRightL">

                            @Html.LabelFor(model => model.StrRID,@Model.StrRID, new { @class = "txtCss"})
                        </td>
                        <td class="textLeftL">清洗人员</td>
                        <td class="textRightL">
                            @Html.LabelFor(model => model.StrCleanUser, @Model.StrCleanUser, new { @class = "txtCss" })
                           
                        </td>

                    </tr>
                    <tr style="height:30px;">
                        <td class="textLeftL">清洗开始时间</td>
                        <td class="textRightL">
                            @Html.LabelFor(model => model.StrCleanSDate, @Model.StrCleanSDate.ToString(), new { @class = "txtCss" })
                            
                        </td>
                        <td class="textLeftL">清洗结束时间</td>
                        <td class="textRightL">
                            @Html.LabelFor(model => model.StrCleanEDate, @Model.StrCleanEDate.ToString(), new { @class = "txtCss" })
                           
                        </td>
                    </tr>

                    <tr style="height:30px;">
                        <td class="textLeftL">故障说明</td>
                        <td class="textRightL" colspan="3">
                            @Html.LabelFor(model => model.StrBreakdown, @Model.StrBreakdown, new { @class = "txtCss", style = "width:93%;" })

                           
                        </td>
                    </tr>
                    <tr style="height:30px;">
                        <td class="textLeftL">备注</td>
                        <td class="textRightL" colspan="3">
                            @Html.LabelFor(model => model.StrRemark, @Model.StrRemark, new { @class = "txtCss", style = "width:93%;" })
                           
                        </td>
                    </tr>
                </table>

               
            </div>
            
        </div>

    }
</body>
</html>
