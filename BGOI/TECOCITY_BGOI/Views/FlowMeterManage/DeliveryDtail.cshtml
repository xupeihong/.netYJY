@{
    Layout = null;
}
@using TECOCITY_BGOI;
@model tk_TakeDelivery

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>DeliveryDtail</title>

    <link href="@Url.Content("~/CSS/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/Print.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery_dialog.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/ui.jqgrid.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")"  type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.jqGrid.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Table_Fix.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/TableMergeCell.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/cmm_PrintPageSet.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/FlowMeterManage/DeliveryDtail.js")" type="text/javascript"></script>
</head>
<body style="margin: 0 0 0 0; background-color: white; overflow: auto;">
    <div id="pageContent" style="margin-left: 2px; margin-right: 2px; margin-top: 2px;">
        <div id="ReportContent" style="margin-top: 5px;">
            @Html.HiddenFor(model => model.strTakeID)
            <div style="display:none;"><span style="margin-left: 10px; float: left;">收货单号：@Model.strTakeID</span></div>
            <div style="width: 98%; margin-left: 10px; margin-top: 5px; page-break-after: always;">
                <div style="text-align: center; width: 100%;">
                    <span style="font-weight: bold; font-size: 20px; text-align: center;">收货确认单
                    </span>
                </div>
                <div style="margin-left: 5px;">
                    <table class="tabInfoWork" id="content" style="text-align: center; margin-top: 5px; width: 100%;" align="center">
                        <tr style="text-align: center;">
                            <td>送货单位基本信息</td>
                            <td>单位名称</td>
                            <td colspan="2" style="text-align: left;">@Model.strUnitName</td>
                            <td>送货人</td>
                            <td>@Model.strDeliverName</td>
                            <td>联系电话</td>
                            <td>@Model.strDeliverTel</td>
                            <td>送货日期</td>
                            <td>@Model.strDeliverDate</td>
                        </tr>
                        <tr style="text-align: center;">
                            <td>仪表基本信息</td>
                            <td>序号</td>
                            <td>类型</td>
                            <td>厂家</td>
                            <td>规格（型号）</td>
                            <td>出厂编号</td>
                            <td>出厂日期</td>
                            <td>连接管径（DN）</td>
                            <td>随表附件</td>
                            <td>外观检查</td>
                        </tr>
                        <tbody id="DetailInfo" class="tabInfoWork"></tbody>
                    </table>
                </div>
                <div style="width: 100%; text-align: left; margin-top: 5px; margin-left: 30px;">
                    收货人&nbsp;&nbsp;&nbsp;&nbsp;@Model.strReceiveName&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                联系方式&nbsp;&nbsp;&nbsp;&nbsp;@Model.strReceiveTel&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                收货日期&nbsp;&nbsp;&nbsp;&nbsp;@Model.strReceiveDate
                </div>
                <div style="text-align: right; margin-top: 5px; width: 99%;">
                    <input type="button" id="btnPrint" value="打 印" class="btn" />
                </div>
                <br />
                <br />
            </div>
        </div>
    </div>
</body>
</html>
