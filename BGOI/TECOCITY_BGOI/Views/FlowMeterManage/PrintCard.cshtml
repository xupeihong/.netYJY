@{
    Layout = null;
}
@using TECOCITY_BGOI;
@model RepairCard

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>PrintCard</title>
    
    <link href="@Url.Content("~/CSS/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/Print.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery_dialog.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/ui.jqgrid.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")"  type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.jqGrid.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/cmm_PrintPageSet.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/FlowMeterManage/PrintCard.js")" type="text/javascript"></script>
</head>
<body style="margin:0 0 0 0; background-color:white; overflow:auto;" >
    <div id="pageContent" style=" margin-left:2px; margin-right:2px; margin-top:2px; ">

        <div id="ReportContent" style="margin-top:10px; page-break-after: always;">
            <div style="width:100%; text-align:center; font-size:18px;">北京公用事业科技发展公司</div>
            <div style="font-size:22px; font-weight:bold; float:left; margin-left:35%;">流量计维修登记卡</div>
            <div style="margin-left:73%; float:left;">维修编号：@Model.strRepairID</div>
            <table id="search" class="tabInfo2">
                @Html.Hidden("RID",ViewData["RID"])
                <tr style="height:30px;">
                    <td>客户名称</td>
                    <td colspan="7" style="text-align:left;">@Model.strCustomerName</td>
                </tr>
                <tr style="height:30px;">
                    <td>客户地址</td>
                    <td colspan="7" style="text-align:left;">@Model.strCustomerAddr</td>
                </tr>
                <tr style="height:30px;">
                    <td>送表人/物流单位</td>
                    <td>@Model.strS_Name</td>
                    <td>联系电话</td>
                    <td colspan="2" style="text-align:left;">@Model.strS_Tel</td>
                    <td>送表日期</td>
                    <td colspan="2" style="text-align:left;">@Model.strS_Date</td>
                </tr>
                <tr style="height:30px;">
                    <td>仪表编号</td>
                    <td>@Model.strMeterID</td>
                    <td>生产厂家</td>
                    <td colspan="2" style="text-align:left;">@Model.strManufacturer</td>
                    <td>精度等级</td>
                    <td colspan="2" style="text-align:left;">@Model.strPrecision</td>
                </tr>
                <tr style="height:30px;">
                    <td>仪表型号</td>
                    <td>@ViewData["Model"]</td>
                    <td>仪表名称</td>
                    <td colspan="2" style="text-align:left;">@Model.strMeterName</td>
                    <td>出厂日期</td>
                    <td colspan="2" style="text-align:left;">@Model.strFactoryDate</td>
                </tr>
                <tr style="height:30px;">
                    <td>字轮读数</td>
                    <td>@Model.strRecordNum</td>
                    <td>流量范围</td>
                    <td>@Model.strFlowRange</td>
                    <td>承压等级</td>
                    <td>@Model.strPressure</td>
                    <td>口径 DN</td>
                    <td>@Model.strCaliber</td>
                </tr>
                <tr style="height: 30px;">
                    <td>仪表性质</td>
                    <td colspan="7" style="text-align:left;">@ViewData["ModelProperty"]</td>
                </tr>
                <tr style="height:30px;">
                    <td colspan="2">基表原使用单位/安装地点</td>
                    <td colspan="6" style="text-align:left;">@Model.strPreUnit</td>
                </tr>
                <tr style="height:30px;">
                    <td colspan="2">基表现使用单位/安装地点</td>
                    <td colspan="6" style="text-align:left;">@Model.strNewUnit</td>
                </tr>
                <tr style="height:30px;">
                    <td>修正仪编号</td>
                    <td>@Model.strX_ID</td>
                    <td>修正仪型号</td>
                    <td colspan="2" style="text-align:left;">@ViewData["X_Model"]</td>
                    <td>生产厂家</td>
                    <td colspan="2" style="text-align:left;">@Model.strX_Manufacturer</td>
                </tr>
                <tr style="height:30px;">
                    <td>标况累计</td>
                    <td>@Model.strX_Standard</td>
                    <td>工况累计</td>
                    <td colspan="2" style="text-align:left;">@Model.strX_Operating</td>
                    <td>生产日期</td>
                    <td colspan="2" style="text-align:left;">@Model.strX_FactoryDate</td>
                </tr>
                <tr style="height:30px;">
                    <td>压力</td>
                    <td>@Model.strX_Pressure</td>
                    <td>温度</td>
                    <td colspan="2" style="text-align:left;">@Model.strX_Temperature</td>
                    <td>其他数据</td>
                    <td colspan="2" style="text-align:left;">@Model.strX_Data</td>
                </tr>
                <tr style="height:30px;">
                    <td colspan="2">修正仪原使用单位/安装地点</td>
                    <td colspan="6" style="text-align:left;">@Model.strX_PreUnit</td>
                </tr>
                <tr style="height:30px;">
                    <td colspan="2">修正仪现使用单位/安装地点</td>
                    <td colspan="6" style="text-align:left;">@Model.strX_NewUnit</td>
                </tr>
                <tr style="height:30px;">
                    <td colspan="5" style="text-align:left;">仪表检查项目：</td>
                    <td>进厂检验员</td>
                    <td colspan="2" style="text-align:left;">@Model.strCheckUser</td>
                </tr>
                <tbody class="tabInfo2" id="CheckItems">

                </tbody>
                
                <tr style="height:30px;">
                    <td>确认维修</td>
                    <td colspan="2">@Html.RadioButton("IsRepair",0) 维修 @Html.RadioButton("IsRepair",1) 不维修</td> @Html.Hidden("StrIsRepair")
                    <td>用户签字</td>
                    <td style="text-align:left;"></td>
                    <td>日期</td>
                    <td colspan="2" >@Model.strConfirmDate</td>
                </tr>
                <tr>
                    @Html.Hidden("HideType",ViewData["GetTypeModel"])
                    <td>取表方式</td>
                    <td>@ViewData["GetTypeModel"]</td>
                    <td>取表日期</td>
                    <td colspan="5">@Model.strG_Date</td>
                </tr>
                <tr style="display: none;" id="Logistic">
                    <td>物流信息</td>
                    <td colspan="7" style="text-align:left;">@Model.strLogistic</td>
                </tr>
                <tr style="display: none;" id="UserInfo">
                    <td>取表人</td>
                    <td>@Model.strG_Name</td>
                    <td>联系电话</td>
                    <td colspan="5" style="text-align:left;">@Model.strG_Tel</td>
                </tr>
                <tr style="height:50px;">
                    <td>备注</td>
                    <td colspan="7" style="text-align:left;">@Model.strText</td>
                </tr>
                    @Html.Hidden("State", ViewData["State"])
                    @Html.Hidden("HTitle")
                    @Html.Hidden("HChecked")
            </table>

        </div>

        <div style="text-align:right; margin-top:5px; width:99%;">
            <input type="button" id="btnPrint" value="打 印" class="btn" />
        </div>
    </div>
</body>
</html>
 