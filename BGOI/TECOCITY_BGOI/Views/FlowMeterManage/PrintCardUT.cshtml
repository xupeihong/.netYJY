@{
    Layout = null;
}
@using TECOCITY_BGOI;
@model RepairCardUT
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>PrintCardUT</title>

    <link href="@Url.Content("~/CSS/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/Print.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery_dialog.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/ui.jqgrid.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")"  type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.jqGrid.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/cmm_PrintPageSet.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/FlowMeterManage/PrintCardUT.js")" type="text/javascript"></script>
</head>
<body style="margin: 0 0 0 0; background-color: white; overflow: auto;">
    <div id="pageContent" style="margin-left: 2px; margin-right: 2px; margin-top: 2px;">

        <div id="ReportContent" style="margin-top: 10px;">
            <div style="width: 100%; text-align: center; font-size: 18px;">北京公用事业科技发展公司</div>
            <div style="font-size: 22px; font-weight: bold; float: left; margin-left: 35%;">超声波流量计登记卡</div>
            <div style="margin-left: 73%; float: left;">编号：@Model.strRepairIDUT</div>
            <table id="search" class="tabInfo2">
                @Html.Hidden("RID", ViewData["RID"])
                <tr style="height: 30px;">
                    <td>客户名称</td>
                    <td colspan="7" style="text-align: left;">@Model.strCustomerNameUT</td>
                </tr>
                <tr style="height: 30px;">
                    <td>客户地址</td>
                    <td colspan="7" style="text-align: left;">@Model.strCustomerAddrUT</td>
                </tr>
                <tr style="height: 30px;">
                    <td>送表人/物流单位</td>
                    <td colspan="2" style="text-align: left;">@Model.strS_NameUT</td>
                    <td>联系电话</td>
                    <td colspan="2" style="text-align: left;">@Model.strS_TelUT</td>
                    <td>送表日期</td>
                    <td>@Model.strS_DateUT</td>
                </tr>
                <tr style="height: 30px;">
                    <td colspan="8">仪表信息</td>
                </tr>
                <tr style="height: 30px;">
                    <td>仪表名称</td>
                    <td colspan="2" style="text-align: left;">@Model.strMeterNameUT</td>
                    <td>生产厂家</td>
                    <td colspan="2" style="text-align: left;">@Model.strManufacturerUT</td>
                    <td>仪表型号</td>
                    <td>@ViewData["Model"]</td>
                </tr>
                <tr style="height: 30px;">
                    <td>仪表编号</td>
                    <td colspan="2" style="text-align: left;">@Model.strMeterIDUT</td>
                    <td>电路板编号</td>
                    <td colspan="2" style="text-align: left;">@Model.strCirNumUT</td>
                    <td>电路板版本</td>
                    <td>@Model.strCirVersionUT</td>
                </tr>
                <tr style="height: 30px;">
                    <td>出厂日期</td>
                    <td>@Model.strFactoryDateUT</td>
                    <td>流量范围</td>
                    <td>@Model.strFlowRangeUT</td>
                    <td>承压等级</td>
                    <td>@Model.strPressureUT</td>
                    <td>口径 DN</td>
                    <td>@Model.strCaliberUT</td>
                </tr>
                <tr style="height: 30px;">
                    <td>仪表性质</td>
                    <td colspan="7" style="text-align:left;">@ViewData["ModelProperty"]</td>
                </tr>
                <tr style="height: 30px;">
                    <td colspan="8">探头信息</td>
                </tr>
                <tr style="height: 30px;">
                    <td>声道</td>
                    <td>1</td>
                    <td>2</td>
                    <td>3</td>
                    <td>4</td>
                    <td>5</td>
                    <td>6</td>
                    <td>备注</td>
                </tr>
                <tr style="height: 30px;">
                    <td style="text-align: center;">A</td>
                    <td>@Model.strTrackA1UT </td>
                    <td>@Model.strTrackA2UT </td>
                    <td>@Model.strTrackA3UT </td>
                    <td>@Model.strTrackA4UT </td>
                    <td>@Model.strTrackA5UT </td>
                    <td>@Model.strTrackA6UT </td>
                    <td rowspan="2">@Model.strComments1UT </td>
                </tr>
                <tr style="height: 30px;">
                    <td style="text-align: center;">B</td>
                    <td>@Model.strTrackB1UT </td>
                    <td>@Model.strTrackB2UT </td>
                    <td>@Model.strTrackB3UT </td>
                    <td>@Model.strTrackB4UT </td>
                    <td>@Model.strTrackB5UT </td>
                    <td>@Model.strTrackB6UT </td>
                </tr>
                <tr style="height: 30px;">
                    <td colspan="8">外观检查</td>
                </tr>
                <tr style="height: 30px;">
                    <td style="text-align: left;" colspan="6">仪表检查项目</td>
                    <td>进厂检验员</td>
                    <td>@Model.strCheckUserUT</td>
                </tr>

                <tbody class="tabInfo2" id="CheckItemsUT">
                </tbody>

                <tr style="height: 30px;">
                    <td colspan="8">测试过程</td>
                </tr>
                <tr style="height: 30px;">
                    <td>首次测试</td>
                    <td>@Html.RadioButton("FirstCheckUT", 0) 合格 @Html.RadioButton("FirstCheckUT", 1) 不合格
                    </td>@Html.Hidden("StrFirstCheckUT")
                    <td>二次测试</td>
                    <td colspan="2">@Html.RadioButton("SecondCheckUT", 0) 合格 @Html.RadioButton("SecondCheckUT", 1) 不合格
                    </td>@Html.Hidden("StrSecondCheckUT")
                    <td>三次测试</td>
                    <td colspan="2">@Html.RadioButton("ThirdCheckUT", 0) 合格 @Html.RadioButton("ThirdCheckUT", 1) 不合格
                    </td>@Html.Hidden("StrThirdCheckUT")
                </tr>
                <tr style="height: 30px;">
                    <td>测试日期</td>
                    <td>@Model.strFirstDateUT</td>
                    <td>测试日期</td>
                    <td colspan="2">@Model.strSecondDateUT</td>
                    <td>测试日期</td>
                    <td colspan="2">@Model.strThirdDateUT</td>
                </tr>
                <tr style="height: 30px;">
                    <td colspan="8">出库记录</td>
                </tr>
                <tr>
                    @Html.Hidden("HideType",ViewData["GetTypeModelUT"])
                    <td>取表方式</td>
                    <td>@ViewData["GetTypeModelUT"]</td>
                    <td>取表日期</td>
                    <td colspan="5">@Model.strG_DateUT</td>
                </tr>
                <tr style="display: none;" id="LogisticUT">
                    <td>物流信息</td>
                    <td colspan="7">@Model.strLogisticUT</td>
                </tr>
                <tr style="height: 30px;" id="UserInfoUT">
                    <td>取表人</td>
                    <td>@Model.strG_NameUT</td>
                    <td>联系电话</td>
                    <td colspan="5">@Model.strG_TelUT</td>
                </tr>
                <tr style="height:50px;">
                    <td>备注</td>
                    <td colspan="7" style="text-align:left;">@Model.strTextUT</td>
                </tr>
            </table>
        </div>

        <div style="text-align: right; margin-top: 5px; width: 99%;">
            <input type="button" id="btnPrint" value="打 印" class="btn" />
        </div>

    </div>
</body>
</html>
