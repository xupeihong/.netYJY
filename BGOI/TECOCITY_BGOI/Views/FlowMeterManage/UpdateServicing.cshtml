@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>UpdateServicing</title>
    <link href="@Url.Content("~/CSS/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery_dialog.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/ui.jqgrid.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Validate/Validate-Injection.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.jqGrid.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/My97DatePicker/WdatePicker.js")" type="text/javascript" charset="utf-8"></script>
    <script src="@Url.Content("~/Scripts/FlowMeterManage/UpdateServicing.js")" type="text/javascript"></script>
</head>
<body>
@using (Html.BeginForm("UpdateServicing", "FlowMeterManage", FormMethod.Post, new { id = "ProjectformInfo", onsubmit = "return returnConfirm();" }))
    {
    <div id="pageContent" style="overflow-y:scroll">
        @Html.Hidden("StrRID")
        @Html.Hidden("BakType")     @Html.Hidden("BakUnitPrice")

        @Html.Hidden("DeviceName")     @Html.Hidden("DeviceType")
        @Html.Hidden("Num")            @Html.Hidden("UnitPrice")
        @Html.Hidden("TotalPrice")     @Html.Hidden("Comments")
        @Html.Hidden("Measure")  
        <div style="width:98%;margin-left:10px;margin-top:5px;">
            <table class="tabInfo" id="content" style="text-align:left">

                <tr>
                    <td colspan="8">分项价格</td>
                </tr>
                <tr>
                    <td colspan="2" style="width:200px">
                        项目
                    </td>
                    <td colspan="2">价格（元）</td>

                </tr>
                <tr style="height: 30px;">
                    <td colspan="2 " style="text-align:left">清洗保养</td>
                    <td colspan="2">
                        <input id="qx" />
                    </td>
                </tr>
                <tr>
                    <td colspan="2">出厂标定</td>
                    <td colspan="2">
                        <input id="ccbd" />
                    </td>
                </tr>

                <tr style="height: 30px;display:none">
                    <td colspan="2">维修</td>
                    <td colspan="2">
                        <input id="wx" />
                    </td>
                </tr>
                <tr>
                    <td colspan="2">代理标定送检</td>
                    <td colspan="2">
                        <input id="dlbd" />
                    </td>
                </tr>


                <tr style="height: 30px;">
                    <td colspan="2">气密性测试（PN10）</td>
                    <td colspan="2">
                        <input id="jc" />
                    </td>
                </tr>
                <tr>
                    <td colspan="2">实流标定保过（确保通过通州、采育2座高压实流标定站的标定）</td>
                    <td colspan="2">
                        <input id="slbd" />
                    </td>
                </tr>
                <tr>
                    <td>其他  法兰盘表面处理（进、出口）</td>
                    <td colspan="7">
                        <input id="qt" />
                    </td>
                </tr>
            </table>
            <div sstyle="overflow-x:auto; width:97%; height:205px; overflow-y:scroll " id="tabL">


                <table class="tabInfo" style="text-align:center;" id="taskList">
                    <tr style="height:30px;">
                        <td class="textRightL" style="text-align:left;" colspan="7"> 更换设备</td>
                    </tr>
                    <tr>
                        <td class="textLeft" style="width:200px;">名称</td>
                        <td class="textLeft" style="width:80px;">规格型号</td>
                        <td class="textLeft" style="width:80px;">数量</td>
                        <td class="textLeft" style="width:80px;">单价</td>
                        <td class="textLeft" style="width:80px;">总价</td>
                        <td class="textLeft" style="width:100px;">备注</td>
                      
                    </tr>
                </table>
            </div>
        </div>

        <div style="float:right; margin-top:10px;">
            <input type="submit" id="QD" class="btn" value="确定" style="margin-right:10px;" />
            <br /><br />
        </div>
    </div>
}
</body>
</html>
