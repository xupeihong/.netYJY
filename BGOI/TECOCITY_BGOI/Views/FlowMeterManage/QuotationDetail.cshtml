@{
    Layout = null;
}
@using TECOCITY_BGOI;
@model tk_Quotation
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>QuotationDetail</title>
    <link href="@Url.Content("~/CSS/Print.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/main.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery_dialog.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/ui.jqgrid.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/jquery-ui-1.10.2.custom.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Validate/Validate-Injection.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-1.9.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.jqGrid.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery_dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/grid.locale-cn.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/My97DatePicker/WdatePicker.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/FlowMeterManage/QuotationDetail.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/cmm_PrintPageSet.js")" type="text/javascript"></script>
 

</head>
<body>
    <div style="width:98%;margin-left:10px;margin-top:5px;">

        <div overflow-x:auto; width:97%; height:205px;" id="tabL">
            @Html.Hidden("BakType")     @Html.Hidden("BakUnitPrice")
            @Html.Hidden("BakConcesioPrice")
            @Html.HiddenFor(model => model.StrRID)
            <table class="tabInfo" style="text-align:center;" id="taskList">
                <tr style="height:30px;">
                    <td class="textRightL" style="text-align:left;" colspan="5"> 报价列表</td>
                </tr>
                <tr>
                    <td class="textLeft" style="width:200px;">报价类型</td>
                    <td class="textLeft" style="width:80px;">价格</td>
                

                </tr>

            </table>
        </div>

        <div overflow-x:auto; width:97%; height:205px;" id="tabL2">
            @Html.Hidden("DeviceName")     @Html.Hidden("DeviceType")
            @Html.Hidden("Measure")        @Html.Hidden("Num")
            @Html.Hidden("UnitPrice")      @Html.Hidden("ConcesioPrice")
            @Html.Hidden("Kind")
            <table class="tabInfo" style="text-align:center;" id="taskList2">
                <tr style="height:30px;">
                    <td class="textRightL" style="text-align:left;" colspan="6"> 更换零件列表</td>
                </tr>
                <tr>
                    <td class="textLeft" style="width:150px;">部件名称</td>
                    <td class="textLeft" style="width:150px;">规格型号</td>
                    <td class="textLeft">数量</td>
                    <td class="textLeft" style="width:80px;">单价</td>
                    <td class="textLeft" style="width:80px;">价格</td>
                    <td class="textLeft" style="width:80px;">备注</td>

                </tr>

            </table>
         
        </div>
        <div style="float:right; margin-top:10px;">
            <input type="button" id="DY" class="btn" value="打印" style="margin-right:10px;" />
            <br /><br />
        </div>
    </div>
</body>
</html>
