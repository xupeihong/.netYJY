                            <div id="divSelFlowRangeUT" class="selDiv" style="top: 22px;" onmouseover="isOut=false;" onmouseout="isOut=true;">
                                <ul id="ulSelFlowRangeUT" style="list-style: none; color: blue; padding-left: 0;">
                                </ul>
                            </div>
                        </td>
                        @*<td class="textRightL">@Html.TextBoxFor(model => model.strFlowRangeUT, new { @class = "txtCss" })</td>*@
                        <td class="textLeftL">承压等级</td>
                        <td style="position: relative;">
                            <input type="text" id="SelPressureUT" class="txtCss" />
                            <div id="divSelPressureUT" class="selDiv" style="top: 22px;" onmouseover="isOut=false;" onmouseout="isOut=true;">
                                <ul id="ulSelPressureUT" style="list-style: none; color: blue; padding-left: 0;">
                                </ul>
                            </div>
                        </td>
                        @*<td class="textRightL">@Html.TextBoxFor(model => model.strPressureUT, new { @class = "txtCss" })</td>*@
                    </tr>
                    <tr style="height: 30px;">
                        <td class="textLeftL">口径DN</td>
                        <td class="textRightL">
                            @Html.TextBoxFor(model => model.strCaliberUT, new { @class = "txtCss" })
                        </td>
                        <td class="textLeftL">仪表性质</td>
                        <td class="textRightL">
                            @Html.DropDownListFor(model => model.strModelPropertyUT, FlowMeterMan.getConfigContent("ModelProperty"), new { @class = "txtCss" })
                        </td>
                    </tr>

                    <tr style="height: 30px;">
                        <td class="textRightL" style="text-align: left;" colspan="4">探头信息</td>
                    </tr>
                    <tr>
                        <td colspan="4">
                            <table class="tabInfo" style="width: 98%;" align="center">
                                <tr style="height: 30px; text-align: center;">
                                    <td class="textLeftL">声道</td>
                                    <td class="textLeftL">1</td>
                                    <td class="textLeftL">2</td>
                                    <td class="textLeftL">3</td>
                                    <td class="textLeftL">4</td>
                                    <td class="textLeftL">5</td>
                                    <td class="textLeftL">6</td>
                                    <td class="textLeftL">备注</td>
                                </tr>
                                <tr style="height: 30px;">
                                    <td style="text-align: center;">A</td>
                                    <td class="textRightL">@Html.TextBoxFor(model => model.strTrackA1UT, new { @class = "txtCss", style = "width:75px;" })</td>
                                    <td class="textRightL">@Html.TextBoxFor(model => model.strTrackA2UT, new { @class = "txtCss", style = "width:75px;" })</td>
                                    <td class="textRightL">@Html.TextBoxFor(model => model.strTrackA3UT, new { @class = "txtCss", style = "width:75px;" })</td>
                                    <td class="textRightL">@Html.TextBoxFor(model => model.strTrackA4UT, new { @class = "txtCss", style = "width:75px;" })</td>
                                    <td class="textRightL">@Html.TextBoxFor(model => model.strTrackA5UT, new { @class = "txtCss", style = "width:75px;" })</td>
                                    <td class="textRightL">@Html.TextBoxFor(model => model.strTrackA6UT, new { @class = "txtCss", style = "width:75px;" })</td>
                                    <td rowspan="2" class="textRightL">
                                        @Html.TextAreaFor(model => model.strComments1UT, new { @class = "txtCss", style = "width:98%; height:60px;" })
                                    </td>
                                </tr>
                                <tr style="height: 30px;">
                                    <td style="text-align: center;">B</td>
                                    <td class="textRightL">@Html.TextBoxFor(model => model.strTrackB1UT, new { @class = "txtCss", style = "width:75px;" })</td>
                                    <td class="textRightL">@Html.TextBoxFor(model => model.strTrackB2UT, new { @class = "txtCss", style = "width:75px;" })</td>
                                    <td class="textRightL">@Html.TextBoxFor(model => model.strTrackB3UT, new { @class = "txtCss", style = "width:75px;" })</td>
                                    <td class="textRightL">@Html.TextBoxFor(model => model.strTrackB4UT, new { @class = "txtCss", style = "width:75px;" })</td>
                                    <td class="textRightL">@Html.TextBoxFor(model => model.strTrackB5UT, new { @class = "txtCss", style = "width:75px;" })</td>
                                    <td class="textRightL">@Html.TextBoxFor(model => model.strTrackB6UT, new { @class = "txtCss", style = "width:75px;" })</td>
                                </tr>
                            </table>
                        </td>

                    </tr>

                    <tr style="height: 30px;">
                        <td class="textRightL" style="text-align: left;" colspan="4">外观检查</td>
                    </tr>
                    <tr style="height: 30px;">
                        <td class="textLeftL">进厂检验员</td>
                        <td class="textRightL" colspan="3" style="text-align: left;">
                            @Html.TextBoxFor(model => model.strCheckUserUT, new { @class = "txtCss" })
                        </td>
                    </tr>
                    <tr style="height: 30px;">
                        <td class="textLeftL">是否送外部检测</td>
                        <td class="textRightL" style="text-align: center;">
                            @Html.RadioButton("IsOutUT", 0, new { onclick = "ShowInfoUT(0);" }) 是 @Html.RadioButton("IsOutUT", 1, new { onclick = "ShowInfoUT(1);" }) 否
                        </td>@Html.Hidden("StrIsOutUT")
                        <td class="textLeftL" id="OutTitleUT" style="display: none;">第三方检测单位</td>
                        <td class="textRightL" id="OutUnitUT" style="display: none;">
                            @Html.TextBoxFor(model => model.strOutUnitUT, new { @class = "txtCss" })
                        </td>
                    </tr>

                    <tbody class="tabInfo" id="CheckItemsUT">
                    </tbody>

                    <tr style="height: 30px;">
                        <td class="textLeftL">外观检查其他项</td>
                        <td colspan="3" class="textRightL">
                            @Html.TextAreaFor(model => model.strFaceOtherUT, new { style = " width:70%; height:45px;" })
                        </td>
                    </tr>
                    <tr style="height: 30px;">
                        <td class="textLeftL">维修内容</td>
                        <td colspan="3" class="textRightL">
                            @Html.TextAreaFor(model => model.strRepairContentUT, new { style = " width:70%; height:45px;" })
                        </td>
                    </tr>
                    <tr style="height: 30px;">
                        <td class="textRightL" style="text-align: left;" colspan="4">测试过程</td>
                    </tr>
                    <tr style="height: 30px;">
                        <td class="textLeftL">首次测试</td>
                        <td class="textRightL">
                            @Html.RadioButton("FirstCheckUT", 0) 合格 
                            @Html.RadioButton("FirstCheckUT", 1) 不合格
                        </td>@Html.Hidden("StrFirstCheckUT")
                        <td class="textLeftL">测试日期</td>
                        <td class="textRightL">
                            @Html.TextBoxFor(model => model.strFirstDateUT, new { onclick = "WdatePicker({dateFmt: 'yyyy-MM-dd' })", @class = "txtCss" })
                        </td>
                    </tr>
                    <tr style="height: 30px;">
                        <td class="textLeftL">二次测试</td>
                        <td class="textRightL">
                            @Html.RadioButton("SecondCheckUT", 0) 合格 
                            @Html.RadioButton("SecondCheckUT", 1) 不合格
                        </td>@Html.Hidden("StrSecondCheckUT")
                        <td class="textLeftL">测试日期</td>
                        <td class="textRightL">
                            @Html.TextBoxFor(model => model.strSecondDateUT, new { onclick = "WdatePicker({dateFmt: 'yyyy-MM-dd' })", @class = "txtCss" })
                        </td>
                    </tr>
                    <tr style="height: 30px;">
                        <td class="textLeftL">三次测试</td>
                        <td class="textRightL">
                            @Html.RadioButton("ThirdCheckUT", 0) 合格 
                            @Html.RadioButton("ThirdCheckUT", 1) 不合格
                        </td>@Html.Hidden("StrThirdCheckUT")
                        <td class="textLeftL">测试日期</td>
                        <td class="textRightL">
                            @Html.TextBoxFor(model => model.strThirdDateUT, new { onclick = "WdatePicker({dateFmt: 'yyyy-MM-dd' })", @class = "txtCss" })
                        </td>
                    </tr>
                    <tr style="height: 30px;">
                        <td class="textRightL" style="text-align: left;" colspan="4">出库记录</td>
                    </tr>
                    <tr style="height: 30px;">
                        <td class="textLeftL">取表方式</td>
                        <td class="textRightL">
                            @Html.DropDownListFor(model => model.strGetTypeModelUT, FlowMeterMan.getConfigContent("GetType"), new { @class = "txtCss", @onchange = "ChangeTypesUT();" })
                        </td>
                        <td class="textLeftL">取表日期</td>
                        <td class="textRightL">
                            @Html.TextBoxFor(model => model.strG_DateUT, new { onclick = "WdatePicker({dateFmt: 'yyyy-MM-dd' })", @class = "txtCss" })
                        </td>
                    </tr>
                    <tr style="height: 30px; display: none;" id="LogisticUT">
                        <td class="textLeftL">物流信息</td>
                        <td colspan="3" class="textRightL">
                            @Html.TextAreaFor(model => model.strLogisticUT, new { style = " width:70%; height:45px;" })
                        </td>
                    </tr>
                    <tr style="height: 30px; display: none;" id="UserInfoUT">
                        <td class="textLeftL">取表人</td>
                        <td class="textRightL">
                            @Html.TextBoxFor(model => model.strG_NameUT, new { @class = "txtCss" })
                        </td>
                        <td class="textLeftL">联系电话</td>
                        <td class="textRightL">@Html.TextBoxFor(model => model.strG_TelUT, new { @class = "txtCss" })</td>
                    </tr>
                    <tr style="height: 30px;">
                        <td class="textLeftL">备注</td>
                        <td colspan="3" class="textRightL">
                            @Html.TextAreaFor(model => model.strTextUT, new { style = " width:70%; height:45px;" })
                        </td>
                    </tr>
                    <tr style="height: 30px;">
                        <td class="textLeftL">收货单号</td>
                        <td class="textRightL">
                            @Html.TextBoxFor(model => model.strTakeIDUT, new { @class = "txtCss" })
                        </td>
                        <td class="textLeftL">发货单号</td>
                        <td class="textRightL">@Html.TextBoxFor(model => model.strDeliverIDUT, new { @class = "txtCss" })</td>
                    </tr>
                    <tr style="height: 30px;">
                        <td class="textLeftL">领取编号</td>
                        <td class="textRightL">
                            @Html.TextBoxFor(model => model.strRecieveIDUT, new { @class = "txtCss" })
                        </td>
                        <td class="textLeftL">承接人（入库单）</td>
                        <td class="textRightL">@Html.TextBoxFor(model => model.strReceiveUserUT, new { @class = "txtCss" })</td>
                    </tr>
                    @Html.Hidden("HTitleUT")
                    @Html.Hidden("HCheckedUT")
                </table>
            </div>

            <div style="width: 98%; margin-left: 15px; margin-top: 10px;">
                注：登记卡登记完成将随即生成随工单和流转卡。
            </div>
            <div style="float: right; margin-top: 10px;">
                <input type="submit" id="QD" class="btn" value="确定" style="margin-right: 10px;" />
                <br />
                <br />
            </div>
        </div>
        
    }
</body>
</html>
